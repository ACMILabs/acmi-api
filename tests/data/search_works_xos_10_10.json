          "headline_credit": "2000",
          "thumbnail": {
            "image_url": "https://acmi-public-api.s3.amazonaws.com/video/works/image/storyboard.jpg.1200x1200_q85.jpg",
            "has_video": false
          },
          "record_type": "work",
          "type": "Film",
          "is_on_display": false,
