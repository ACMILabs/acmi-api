hy": "", "biography_author": null, "date_of_biography": "2023-08-11T13:38:10.659973+10:00", "external_links": [], "uuid": "9a502db1-9358-439f-9505-2f8e946fb36d", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "4682", "external_references": [], "date_modified": "2025-03-06T23:06:54.051892+11:00"}, {"id": 12130, "name": "Frank Stauffacher", "roles_in_work": [{"id": 858048, "title": "Sausalito", "work_id": 72490, "role": "director", "role_id": 938, "is_primary": true}, {"id": 2676702, "title": "Adventures of Jimmy", "work_id": 64555, "role": "cinematographer", "role_id": 32, "is_primary": false}, {"id": 1813595, "title": "Notes on the Port of St. Francis", "work_id": 71000, "role": "producer", "role_id": 1089, "is_primary": true}], "also_known_as": "", "date_of_birth": null, "date_of_death": "1955-07-24", "genders": [{"id": 9, "name": "Male", "slug": "male"}], "languages": [], "places_of_operation": [{"id": 1, "name": "United States of America", "slug": "united-states-of-america"}], "biography": "Frank Stauffacher (1917 – 24 July 1955, in San Francisco, California) was an American experimental filmmaker, best known for directing the cinema series \"Art in Cinema\" at the San Francisco Museum of Modern Art from 1946 to 1954.He was the cinematographer for Mother's Day (1948) and Adventures of Jimmy (1950), two films by James Broughton. His brother, Jack Stauffacher, is a well-known printer and typeface creator. On December 18, 2013, Notes on the Port of St. Francis was selected for the Library of Congress's National Film Registry.\nFrom November 1948 until his death at age 38 from a brain tumor, Stauffacher was married to graphic artist Barbara Stauffacher Solomon.\nStauffacher's short film Zigzag was preserved by the Academy Film Archive in 2013.", "biography_author": "Wikidata", "date_of_biography": "2023-08-01T14:52:12.057849+10:00", "external_links": [{"id": 3994, "uri": "https://en.wikipedia.org/wiki/Frank_Stauffacher"}], "uuid": "e35aee0f-6af4-4b23-82a4-e6ac5cd9edd0", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "19175", "external_references": [{"id": 26521, "source": {"name": "Wikidata", "slug": "wikidata"}, "source_identifier": "Q5489755"}, {"id": 28464, "source": {"name": "VIAF", "slug": "viaf"}, "source_identifier": "57386704"}, {"id": 28465, "source": {"name": "LOC Auth", "slug": "loc-auth"}, "source_identifier": "no2003026076"}, {"id": 28466, "source": {"name": "WorldCat", "slug": "worldcat"}, "source_identifier": "lccn-no2003026076"}], "date_modified": "2025-03-06T23:06:51.867409+11:00"}, {"id": 12129, "name": "James Broughton", "roles_in_work": [{"id": 857035, "title": "The Pleasure garden", "work_id": 71666, "role": "director", "role_id": 938, "is_primary": true}, {"id": 855659, "title": "Mother's day", "work_id": 70607, "role": "director", "role_id": 938, "is_primary": true}, {"id": 854753, "title": "Loony Tom: the happy lover", "work_id": 69918, "role": "producer/director", "role_id": 31, "is_primary": true}, {"id": 2714144, "title": "Four in the afternoon", "work_id": 67764, "role": "producer/director", "role_id": 31, "is_primary": true}, {"id": 2676700, "title": "Adventures of Jimmy", "work_id": 64555, "role": "producer/director", "role_id": 31, "is_primary": true}], "also_known_as": "", "date_of_birth": "1913-11-10", "date_of_death": "1999-05-17", "genders": [{"id": 9, "name": "Male", "slug": "male"}], "languages": [{"id": 1, "name": "English", "slug": "english"}], "places_of_operation": [{"id": 1, "name": "United States of America", "slug": "united-states-of-america"}], "biography": "James Broughton (November 10, 1913 – May 17, 1999) was an American poet and poetic filmmaker. He was part of the San Francisco Renaissance, a precursor to the Beat poets. He was an early bard of the Radical Faeries, as well as a member of The Sisters of Perpetual Indulgence, serving the community as Sister Sermonetta.\n\n", "biography_author": "Wikidata", "date_of_biography": "2023-08-0