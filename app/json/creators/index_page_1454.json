_work": [{"id": 815388, "title": "Lagaan: Once upon a time in India [NTSC]", "work_id": 93409, "role": "director", "role_id": 938, "is_primary": true}], "also_known_as": "", "date_of_birth": "1964-02-15", "date_of_death": null, "genders": [{"id": 9, "name": "Male", "slug": "male"}], "languages": [{"id": 40, "name": "Hindi", "slug": "hindi"}], "places_of_operation": [{"id": 50, "name": "India", "slug": "india"}], "biography": "Ashutosh Gowariker (born 15 February 1964) is an Indian film director, actor, screenwriter and producer who works in Hindi cinema. He is known for directing films \"set on a huge canvas while boasting of an opulent treatment\".He is particularly known for Lagaan (2001), Swades (2004), Jodhaa Akbar (2008), and Mohenjo Daro (2016).\nHis film Lagaan was nominated in the Best Foreign Language Film category in 74th Academy Awards, which makes him a voting member of the Academy Awards. He returned to acting by playing the lead role in the critically acclaimed comedy drama Ventilator (2016).", "biography_author": "Wikidata", "date_of_biography": "2023-08-01T14:07:35.280538+10:00", "external_links": [{"id": 3597, "uri": "https://en.wikipedia.org/wiki/Ashutosh_Gowariker"}], "uuid": "ad3231e0-ca36-4421-8ab6-8ec9e2fd7f8c", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "15577", "external_references": [{"id": 26282, "source": {"name": "Wikidata", "slug": "wikidata"}, "source_identifier": "Q281249"}, {"id": 27381, "source": {"name": "VIAF", "slug": "viaf"}, "source_identifier": "194906851"}, {"id": 27382, "source": {"name": "LOC Auth", "slug": "loc-auth"}, "source_identifier": "no96009510"}, {"id": 27383, "source": {"name": "WorldCat", "slug": "worldcat"}, "source_identifier": "lccn-no96009510"}], "date_modified": "2023-09-13T12:28:20.246352+10:00"}]}