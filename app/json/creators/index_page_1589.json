f_death": null, "genders": [{"id": 5, "name": "Female", "slug": "female"}], "languages": [{"id": 1, "name": "English", "slug": "english"}], "places_of_operation": [{"id": 10, "name": "United Kingdom", "slug": "united-kingdom"}], "biography": "Hettie Macdonald is an English film, theatre and television director. Macdonald is known as the director of the Hugo Award-winning 2007 episode of the Doctor Who series, \"Blink\", and won a Grand Prix award, an International Jury Award - Honorable Mention and a People's Choice Award for her work on the film Beautiful Thing.\n\"Blink\" is frequently named as the best episode of Doctor Who since the series' 2005 revival. In 2009, SFX named the episode's climax as the scariest moment in Doctor Who's history, citing its \"perfect direction\". Macdonald would return to the series in 2015 to direct the year's opening story.\nShe has also directed for the stage. She studied English at Bristol University before training as a director at the Royal Court Theatre, and was formerly associate director at the Wolsey Theatre, Ipswich.", "biography_author": "Wikidata", "date_of_biography": "2022-05-19T11:45:42.495505+10:00", "external_links": [{"id": 2236, "uri": "https://en.wikipedia.org/wiki/Hettie_Macdonald"}], "uuid": "e7e365a4-679a-4153-b1b1-a7cc790599ed", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "20542", "external_references": [{"id": 9605, "source": {"name": "Wikidata", "slug": "wikidata"}, "source_identifier": "Q5748037"}, {"id": 9606, "source": {"name": "VIAF", "slug": "viaf"}, "source_identifier": "51886631"}, {"id": 9607, "source": {"name": "LOC Auth", "slug": "loc-auth"}, "source_identifier": "no2007089274"}], "date_modified": "2023-09-13T12:29:59.094912+10:00"}, {"id": 31129, "name": "Magnum Entertainment", "roles_in_work": [{"id": 2357640, "title": "The Killer", "work_id": 89209, "role": "production company", "role_id": 941, "is_primary": true}], "also_known_as": "", "date_of_birth": "", "date_of_deat