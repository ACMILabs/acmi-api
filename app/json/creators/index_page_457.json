any Moana Productions.", "biography_author": "Wikidata", "date_of_biography": "2023-09-05T23:44:19.264841+10:00", "external_links": [{"id": 4826, "uri": "https://en.wikipedia.org/wiki/Paul_Atkins_(cinematographer)"}], "uuid": "ac013903-1b1c-45ac-97cc-3fa5de86012d", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "26164", "external_references": [{"id": 27011, "source": {"name": "Wikidata", "slug": "wikidata"}, "source_identifier": "Q7149123"}], "date_modified": "2023-09-05T23:44:19.275129+10:00"}, {"id": 80936, "name": "Grace Atkins", "roles_in_work": [{"id": 2476102, "title": "Great white shark", "work_id": 94661, "role": "producer", "role_id": 1089, "is_primary": true}], "also_known_as": "", "date_of_birth": "", "date_of_death": "", "genders": [], "languages": [], "places_of_operation": [], "biography": "", "biography_author": null, "date_of_biography": "2023-08-17T15:32:19.882471+10:00", "external_links": [], "uuid": "263c1728-6962-4716-8332-97cb2b73ade9", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "19968", "external_references": [{"id": 34579, "source": {"name": "Wikidata", "slug": "wikidata"}, "source_identifier": "Q121144089"}], "date_modified": "2023-09-05T20:18:06.062002+10:00"}, {"id": 80935, "name": "Melanie Archer", "roles_in_work": [{"id": 916290, "title": "The Spartans", "work_id": 94660, "role": "co-producers/directors", "role_id": 628, "is_primary": true}], "also_known_as": "", "date_of_birth": null, "date_of_death": null, "genders": [], "languages": [], "places_of_operation": [], "biography": "", "biography_author": null, "date_of_biography": null, "external_links": [], "uuid": "9f684661-d1f8-4458-b2f5-3716d439f889", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": null, "external_references": [], "date_modified": "2023-06-14T16:33:30.576250+10:00"}, {"id": 80934, "name": "Tim Kirby", "roles_in_work": [{"id": 916289, "title": "The Spartans", "work_id": 94660, "role": "co-producers/dire