841, "name": "Viking Eggeling", "roles_in_work": [{"id": 2636064, "title": "Symphonie Diagonale", "work_id": 108728, "role": "creator", "role_id": 968, "is_primary": true}, {"id": 2636065, "title": "Symphonie Diagonale", "work_id": 108728, "role": "animator", "role_id": 497, "is_primary": false}, {"id": 2636068, "title": "Selection of early silent experimental films (Walter Ruttmann, Viking Eggeling and Hans Richter)", "work_id": 108727, "role": "creator", "role_id": 968, "is_primary": true}], "also_known_as": "", "date_of_birth": "", "date_of_death": "", "genders": [], "languages": [], "places_of_operation": [], "biography": "Swedish artist Viking Eggeling (1880-1925) studied art history and painting in Germany before moving to Switzerland at the outbreak of World War I. He became involved in the Dadaist movement, befriending Tristan Tzara and Jean Arp, and then returned to Germany after the war. Fascinated with film, he began experimenting with animation, developing a unique abstract style using paper cut-outs and tin foil figures. Symphonie Diagonale, his first major work was completed in 1924 but, sadly, Eggeling died in Berlin shortly after the film¿s premiere public screening.", "biography_author": null, "date_of_biography": "2025-03-05T10:32:12.426986+11:00", "external_links": [], "uuid": "4fb9c34f-b93b-4d16-b03e-5e098dc32f92", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "111814", "external_references": [], "date_modified": "2025-03-05T10:32:12.427309+11:00"}]}