ter.", "biography_author": "Wikidata", "date_of_biography": "2023-08-01T17:19:36.125614+10:00", "external_links": [{"id": 5277, "uri": "https://en.wikipedia.org/wiki/Tom_Gurr"}], "uuid": "7feee718-fdb9-4708-b35d-8e8f8b9015fc", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "29837", "external_references": [{"id": 25723, "source": {"name": "Wikidata", "slug": "wikidata"}, "source_identifier": "Q7816038"}], "date_modified": "2023-09-13T12:33:02.589544+10:00"}, {"id": 16079, "name": "Commonwealth Department of Information", "roles_in_work": [{"id": 2392818, "title": "Australian diary. No. 009", "work_id": 96151, "role": "production company", "role_id": 941, "is_primary": true}, {"id": 2392156, "title": "Surf Patrol", "work_id": 73396, "role": "production company", "role_id": 941, "is_primary": true}, {"id": 2392713, "title": "Pacific terminal", "work_id": 88470, "role": "production company", "role_id": 941, "is_primary": true}, {"id": 853974, "title": "Jungle patrol", "work_id": 69320, "role": "production company", "role_id": 941, "is_primary": true}], "also_known_as": "", "date_of_birth": "", "date_of_death": "", "genders": [], "languages": [], "places_of_operation": [], "biography": "", "biography_author": null, "date_of_biography": "2023-08-09T04:09:04.391052+10:00", "external_links": [], "uuid": "4d5a20fa-8266-4461-afa2-b365b4516114", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "6935", "external_references": [{"id": 39296, "source": {"name": "Wikidata", "slug": "wikidata"}, "source_identifier": "Q121126901"}], "date_modified": "2023-09-05T18:44:29.503438+10:00"}, {"id": 16077, "name": "Computer Image", "roles_in_work": [{"id": 2153058, "title": "Jump rope", "work_id": 69315, "role": "production company", "role_id": 941, "is_primary": true}], "also_known_as": "", "date_of_birth": "", "date_of_death": "", "genders": [], "languages": [], "places_of_operation": [], "biography": "", "biography_author": null, "date_of_biograp