tery.\n\n", "biography_author": "Wikidata", "date_of_biography": "2022-05-19T10:42:04.613831+10:00", "external_links": [{"id": 1567, "uri": "https://en.wikipedia.org/wiki/Alain_Corneau"}], "uuid": "85790d50-ad7e-477c-b34c-4f7ef050ead6", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "14715", "external_references": [{"id": 6871, "source": {"name": "Wikidata", "slug": "wikidata"}, "source_identifier": "Q312280"}, {"id": 6872, "source": {"name": "VIAF", "slug": "viaf"}, "source_identifier": "43044212"}, {"id": 6873, "source": {"name": "LOC Auth", "slug": "loc-auth"}, "source_identifier": "no97055634"}, {"id": 6874, "source": {"name": "WorldCat", "slug": "worldcat"}, "source_identifier": "lccn-no97055634"}], "date_modified": "2024-10-23T03:41:13.814908+11:00"}]}