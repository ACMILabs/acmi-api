ate_modified": "2025-03-13T01:26:43.071775+11:00"}, {"id": 20627, "name": "New Cinema Workshop", "roles_in_work": [{"id": 2415105, "title": "Linderidge 137", "work_id": 77647, "role": "production company", "role_id": 941, "is_primary": true}], "also_known_as": "", "date_of_birth": "", "date_of_death": "", "genders": [], "languages": [], "places_of_operation": [], "biography": "", "biography_author": null, "date_of_biography": "2023-08-11T19:30:46.800921+10:00", "external_links": [], "uuid": "956b7dd2-8eaf-421b-9c5a-171c45ffd28c", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "10891", "external_references": [], "date_modified": "2023-08-11T19:30:46.801168+10:00"}, {"id": 20622, "name": "London Filmmaker's Co-op", "roles_in_work": [{"id": 2151870, "title": "Married Life", "work_id": 77637, "role": "production company", "role_id": 941, "is_primary": true}], "also_known_as": "", "date_of_birth": "", "date_of_death": "", "genders": [], "languages": [], "places_of_operation": [], "biography": "", "biography_author": null, "date_of_biography": "2023-05-09T13:53:56.663699+10:00", "external_links": [], "uuid": "bb9e2ee7-fde8-40f2-8c14-7bc68f9f23cd", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "10135", "external_references": [{"id": 41316, "source": {"name": "Wikidata", "slug": "wikidata"}, "source_identifier": "Q121143787"}], "date_modified": "2023-09-05T22:29:29.402436+10:00"}, {"id": 20620, "name": "Andrei Konchalovsky", "roles_in_work": [{"id": 1984919, "title": "Asya's Happiness", "work_id": 80902, "role": "director", "role_id": 938, "is_primary": true}, {"id": 2647872, "title": "Split cherry tree", "work_id": 77635, "role": "director", "role_id": 938, "is_primary": true}], "also_known_as": "Andron Sergheievici Mihalkov", "date_of_birth": "1937-08-20", "date_of_death": null, "genders": [{"id": 9, "name": "Male", "slug": "male"}], "languages": [{"id": 1, "name": "English", "slug": "english"}, {"id": 5, "name": "German", "slug": "german"}, {"id": 3, "name": "Russian", "slug": "russian"}], "places_of_operation": [{"id": 180, "name": "Soviet Union", "slug": "soviet-union"}], "biography": "Andrei Sergeyevich Konchalovsky OZO (Russian: Андрей Сергеевич Кончаловский; born 20 August 1937) is a Russian filmmaker. He has worked in Soviet, Hollywood, and contemporary Russian cinema. He is a laureate of the Order \"For Merit to the Fatherland\", a National Order of the Legion of Honour, an Officer of the Order of Arts and Letters, a Cavalier of the Order of Merit of the Italian Republic and a People's Artist of the RSFSR. He is the son of writer Sergey Mikhalkov, and the brother of filmmaker Nikita Mikhalkov.\nKonchalovsky's work has encompassed theatrical motion pictures, telefilms, documentaries, and stage productions. His film credits include Uncle Vanya (1970), Siberiade (1979), Maria's Lovers  (1984), Runaway Train (1985), Tango & Cash (1989), House of Fools (2002), The Postman's White Nights (2014), Paradise (2016), and Dear Comrades! (2020). He also directed the 1997 miniseries adaptation of the ancient Greek narrative The Odyssey. Earlier in his career, he was a collaborator of Andrei Tarkovsky. His films have won numerous accolades, including the Cannes Grand Prix Spécial du Jury, a FIPRESCI Award, two Silver Lions, three Golden Eagle Awards, and a Primetime Emmy Award.", "biography_author": "Wikidata", "date_of_biography": "2023-08-01T14:01:37.244790+10:00", "external_links": [{"id": 3545, "uri": "https://en.wikipedia.org/wiki/Andrei_Konchalovsky"}], "uuid": "17cbb45c-3ea9-424e-ba4b-d146e48c9dd2", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "15134", "external_references": [{"id": 26718, "source": {"name": "Wikidata", "slug": "wikidata"}, "source_identifier": "Q55208"}, {"id": 27237, "source": {"name": "VIAF", "slug": "viaf"}, "source_identifier": "4903670"}, {"id": 27238, "source": {"name": "LOC Auth", "slug": "loc-auth"}, "source_identifier": "n78033069"}, {"id