ion": [], "biography": "", "biography_author": null, "date_of_biography": "2023-05-09T20:08:15.686521+10:00", "external_links": [], "uuid": "8def22b8-031f-4000-a998-65dd3e561b97", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "34456", "external_references": [], "date_modified": "2023-06-14T16:33:30.576250+10:00"}, {"id": 81914, "name": "Guan Wei", "roles_in_work": [{"id": 2342735, "title": "Paper War", "work_id": 114190, "role": "producer/director", "role_id": 31, "is_primary": true}], "also_known_as": "", "date_of_birth": "1957", "date_of_death": "", "places_of_operation": [], "biography": "Born 1957 Beijing, lives and works in Sydney.\nGuan Wei graduated in 1986 from the Department of Fine Arts at Beijing Capital University. He visited Australia in 1989 for a residency at the Tasmanian School of Art, Hobart, and undertook further residencies at the Museum of Contemporary Art, Sydney, and the Canberra School of Art before becoming an Australian resident in 1993. He has held thirty solo exhibitions and his work has been included in numerous contemporary art exhibitions in Australia and internationally. He is the recipient of grants from the Australia Council and Asialink, and has won many awards including the Mosman Art Prize, 2001; the 39th Festival of Fisher's Ghost Award, 2001; and the Sulman Prize, 2002.", "biography_author": null, "date_of_biography": "2023-05-09T19:42:20.293498+10:00", "external_links": [], "uuid": "a8def4ce-cebd-4d3b-8200-3182b7fe28da", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "2053", "external_references": [], "date_modified": "2023-06-14T16:33:30.576250+10:00"}, {"id": 81913, "name": "Guo Jian", "roles_in_work": [{"id": 2342702, "title": "The Day Before I Went Away", "work_id": 114189, "role": "producer/director", "role_id": 31, "is_primary": true}], "also_known_as": "", "date_of_birth": "1963", "date_of_death": "", "places_of_operation": [], "biography": "Born 1963 Guizhou, China; lives and works in Sydney.\nGuo Jian entered the People's Liberation Army in 1979 as a squad leader and army artist. After retiring he worked as a propaganda artist in a government transportation company. In 1985 he began studying traditional Chinese art at the Central Minorities Institute, Beijing, and graduated in 1989 after the Tiananmen Square massacre. As punishment for his involvement in the hunger strike, he was sent to work in a domestic goods factory. Rejecting this, he began working independently amongst the Yuan Ming Yuan community of artists, immigrating to Australia in 1992. He has held six solo exhibitions and numerous group exhibitions. He is represented by Ray Hughes Gallery, Sydney.", "biography_author": null, "date_of_biography": "2023-05-09T19:42:17.151161+10:00", "external_links": [], "uuid": "94fbae69-d378-429d-a102-e055deb3bbe7", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "2046", "external_references": [], "date_modified": "2023-06-14T16:33:30.576250+10:00"}, {"id": 81912, "name": "Tobias Grime", "roles_in_work": [{"id": 1765798, "title": "Godspeed Verticals", "work_id": 114182, "role": "composer", "role_id": 538, "is_primary": false}], "also_known_as": "", "date_of_birth": null, "date_of_death": null, "places_of_operation": [], "biography": "", "biography_author": null, "date_of_biography": null, "external_links": [], "uuid": "608cc47d-87fe-4615-a19b-69434f86b6d3", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": null, "external_references": [], "date_modified": "2023-06-14T16:33:30.576250+10:00"}]}