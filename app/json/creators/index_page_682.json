: [{"id": 2475588, "title": "Mackenna's gold", "work_id": 87878, "role": "co-producer", "role_id": 915, "is_primary": true}], "also_known_as": "", "date_of_birth": "1894-05-10", "date_of_death": "1979-11-11", "genders": [{"id": 9, "name": "Male", "slug": "male"}], "languages": [], "places_of_operation": [{"id": 1, "name": "United States of America", "slug": "united-states-of-america"}], "biography": "Dimitri Zinovievich Tiomkin (May 10, 1894 – November 11, 1979) was a Russian-born American film composer and conductor. Classically trained in Saint Petersburg before the Bolshevik Revolution, he moved to Berlin and then New York City after the Russian Revolution. In 1929, after the stock market crash, he moved to Hollywood, where he became best known for his scores for Western films, including Duel in the Sun, Red River, High Noon, The Big Sky, Gunfight at the O.K. Corral, Rio Bravo, and Last Train from Gun Hill.\nTiomkin received 22 Academy Award nominations and won four Oscars, three for Best Original Score for High Noon, The High and the Mighty, and The Old Man and the Sea, and one for Best Original Song for \"The Ballad of High Noon\" from the film High Noon.\n\n", "biography_author": "Wikidata", "date_of_biography": "2023-09-05T19:12:41.687572+10:00", "external_links": [{"id": 1934, "uri": "https://en.wikipedia.org/wiki/Dimitri_Tiomkin"}], "uuid": "edc323b3-852a-492e-901e-97a81ace8f70", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "18151", "external_references": [{"id": 8387, "source": {"name": "Wikidata", "slug": "wikidata"}, "source_identifier": "Q317033"}, {"id": 8388, "source": {"name": "VIAF", "slug": "viaf"}, "source_identifier": "100251573"}, {"id": 8389, "source": {"name": "LOC Auth", "slug": "loc-auth"}, "source_identifier": "n2006010255"}, {"id": 8390, "source": {"name": "WorldCat", "slug": "worldcat"}, "source_identifier": "lccn-n2006010255"}], "date_modified": "2023-09-05T19:12:43.627283+10:00"}]}