ternal_references": [{"id": 35603, "source": {"name": "Wikidata", "slug": "wikidata"}, "source_identifier": "Q121058105"}], "date_modified": "2025-03-13T14:49:54.460851+11:00"}, {"id": 74736, "name": "Edmund Hillary", "roles_in_work": [{"id": 2714691, "title": "The Glacier climbers", "work_id": 68068, "role": "cast", "role_id": 33, "is_primary": false}], "also_known_as": "", "date_of_birth": "1919", "date_of_death": "", "genders": [{"id": 9, "name": "Male", "slug": "male"}], "languages": [], "places_of_operation": [], "biography": "", "biography_author": null, "date_of_biography": "2023-05-09T19:56:41.172851+10:00", "external_links": [], "uuid": "681408ff-fb1f-4992-9449-12d15846e0d1", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "34067", "external_references": [{"id": 39271, "source": {"name": "Wikidata", "slug": "wikidata"}, "source_identifier": "Q121057818"}], "date_modified": "2025-03-12T01:17:14.985939+11:00"}, {"id": 74735, "name": "Shel Silverstein", "roles_in_work": [{"id": 2351402, "title": "The Giving tree", "work_id": 86034, "role": "co-producer", "role_id": 915, "is_primary": true}, {"id": 2644961, "title": "The Giving tree", "work_id": 68067, "role": "co-producer", "role_id": 915, "is_primary": true}], "also_known_as": "Sheldon Allan Silverstein", "date_of_birth": "1930-09-25", "date_of_death": "1999-05-10", "genders": [{"id": 9, "name": "Male", "slug": "male"}], "languages": [{"id": 1, "name": "English", "slug": "english"}], "places_of_operation": [{"id": 1, "name": "United States of America", "slug": "united-states-of-america"}], "biography": "Sheldon Allan Silverstein (; September 25, 1930 – May 10, 1999) was an American writer, poet, cartoonist, singer-songwriter, musician, and playwright. Born and raised in Chicago, Illinois, Silverstein briefly attended university before being drafted into the United States Army. During his rise to prominence in the 1950s, his illustrations were published in various newspapers and magazines, including the adult-oriented Playboy. He also wrote a satirical, adult-oriented alphabet book, Uncle Shelby's ABZ Book, under the stylized name \"Uncle Shelby\", which he used as an occasional pen name.\nAs a children's author, some of his most acclaimed works include The Giving Tree, Where the Sidewalk Ends, and A Light in the Attic. His works have been translated into more than 47 languages and have sold more than 20 million copies. As a songwriter, Silverstein wrote the 1969 Johnny Cash track \"A Boy Named Sue\", which peaked at number 2 on the U.S. Billboard Hot 100. His songs have been recorded and popularized by a wide range of other acts including Tompall Glaser, The Irish Rovers and Dr. Hook & the Medicine Show. He was the recipient of two Grammy Awards as well as nominations at the Golden Globe Awards and Academy Awards.\nSilverstein had two children, Shoshanna Jordan Hastings (June 30, 1970 – April 24, 1982) and Matthew De Ver (born November 10, 1984). Shoshanna died of an aneurysm at age 11, and the book A Light in the Attic is dedicated to her memory. Silverstein died at home in Key West, Florida, of a heart attack on May 10, 1999, at age 68.", "biography_author": "Wikidata", "date_of_biography": "2023-09-06T01:14:27.297520+10:00", "external_links": [{"id": 6176, "uri": "https://en.wikipedia.org/wiki/Shel_Silverstein"}], "uuid": "36fe46cd-a479-4b5c-9f30-7bfde06c9706", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "28733", "external_references": [{"id": 51756, "source": {"name": "Wikidata", "slug": "wikidata"}, "source_identifier": "Q361257"}, {"id": 55807, "source": {"name": "VIAF", "slug": "viaf"}, "source_identifier": "19880380"}, {"id": 55808, "source": {"name": "LOC Auth", "slug": "loc-auth"}, "source_identifier": "n79045309"}, {"id": 55809, "source": {"name": "WorldCat", "slug": "worldcat"}, "source_identifier": "lccn-n79045309"}], "date_modified": "2025-03-05T12:30:30.204183+11:00"}, {"id": 74734, "name": "Bruce Sharman", "r