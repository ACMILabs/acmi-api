dia.org/wiki/Donald_Shebib"}], "uuid": "a3924994-150d-4828-9077-1b10ea836bb3", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "18313", "external_references": [{"id": 26600, "source": {"name": "Wikidata", "slug": "wikidata"}, "source_identifier": "Q1240358"}, {"id": 28236, "source": {"name": "VIAF", "slug": "viaf"}, "source_identifier": "501149196275174790764"}, {"id": 28237, "source": {"name": "LOC Auth", "slug": "loc-auth"}, "source_identifier": "n79015270"}, {"id": 28238, "source": {"name": "WorldCat", "slug": "worldcat"}, "source_identifier": "lccn-n79015270"}], "date_modified": "2023-09-05T19:18:31.224358+10:00"}, {"id": 75284, "name": "Gaston Serault", "roles_in_work": [{"id": 1803614, "title": "The Sand castle", "work_id": 72471, "role": "producer", "role_id": 1089, "is_primary": true}], "also_known_as": "", "date_of_birth": null, "date_of_death": null, "genders": [], "languages": [], "places_of_operation": [], "biography": "", "biography_author": null, "date_of_biography": null, "external_links": [], "uuid": "453f8533-2b5e-461e-99a5-482a411fce6b", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": null, "external_references": [{"id": 32579, "source": {"name": "Wikidata", "slug": "wikidata"}, "source_identifier": "Q121035948"}], "date_modified": "2023-09-05T19:59:19.736415+10:00"}]}