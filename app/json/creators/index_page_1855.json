_in_work": [{"id": 1986774, "title": "The Running man (Captioned)", "work_id": 83036, "role": "director", "role_id": 938, "is_primary": true}], "also_known_as": "", "date_of_birth": "1943-03-25", "date_of_death": null, "genders": [{"id": 9, "name": "Male", "slug": "male"}], "languages": [{"id": 1, "name": "English", "slug": "english"}], "places_of_operation": [{"id": 1, "name": "United States of America", "slug": "united-states-of-america"}], "biography": "Paul Michael Glaser (born Paul Manfred Glaser; March 25, 1943) is an American actor and director best known for his role as Detective Dave Starsky on the 1970s television series, Starsky & Hutch. In between his work writing and directing, Glaser also played Captain Jack Steeper on the NBC series Third Watch from 2004 to 2005, appeared as Al in several episodes of Ray Donovan in the 2010s, and had his first U.S. exhibition of his artwork in 2018.", "biography_author": "Wikidata", "date_of_biography": "2023-08-01T16:30:42.177031+10:00", "external_links": [{"id": 4848, "uri": "https://en.wikipedia.org/wiki/Paul_Michael_Glaser"}], "uuid": "c812a4ec-d5ad-46ba-99f0-d00776c84bde", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "26292", "external_references": [{"id": 26362, "source": {"name": "Wikidata", "slug": "wikidata"}, "source_identifier": "Q527306"}, {"id": 30707, "source": {"name": "VIAF", "slug": "viaf"}, "source_identifier": "34643234"}, {"id": 30708, "source": {"name": "LOC Auth", "slug": "loc-auth"}, "source_identifier": "n81124525"}, {"id": 30709, "source": {"name": "WorldCat", "slug": "worldcat"}, "source_identifier": "lccn-n81124525"}], "date_modified": "2023-09-13T12:31:48.295202+10:00"}]}