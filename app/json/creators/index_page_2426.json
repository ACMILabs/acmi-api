phy_author": null, "date_of_biography": "2023-08-09T03:58:40.829466+10:00", "external_links": [], "uuid": "25bbf333-e3e5-43c9-90cf-591828301ebb", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "501", "external_references": [{"id": 33560, "source": {"name": "Wikidata", "slug": "wikidata"}, "source_identifier": "Q121299407"}], "date_modified": "2023-08-09T03:58:40.829745+10:00"}, {"id": 10025, "name": "Joan Long", "roles_in_work": [{"id": 2392385, "title": "The Passionate industry: 1920-1930", "work_id": 75260, "role": "director", "role_id": 938, "is_primary": true}, {"id": 2392032, "title": "Rail town", "work_id": 72035, "role": "script", "role_id": 962, "is_primary": false}, {"id": 2392029, "title": "Rail town", "work_id": 72035, "role": "director", "role_id": 938, "is_primary": true}, {"id": 2311182, "title": "Turnover", "work_id": 74096, "role": "script", "role_id": 962, "is_primary": false}, {"id": 2299056, "title": "Silver City", "work_id": 80494, "role": "producer", "role_id": 1089, "is_primary": true}, {"id": 2232066, "title": "Puberty blues [DVD]", "work_id": 94351, "role": "co-producer", "role_id": 915, "is_primary": true}, {"id": 2232050, "title": "Puberty blues", "work_id": 79916, "role": "co-producer", "role_id": 915, "is_primary": true}, {"id": 2216492, "title": "Emerald city [DVD]", "work_id": 93206, "role": "producer", "role_id": 1089, "is_primary": true}, {"id": 1986114, "title": "The Picture Show Man", "work_id": 81896, "role": "producer", "role_id": 1089, "is_primary": true}], "also_known_as": "", "date_of_birth": "", "date_of_death": "", "genders": [], "languages": [], "places_of_operation": [], "biography": "Joan Long (née Boundy) began her career in the early 1950s as a secretary with the Commonwealth Film Unit. In 1952 she became the second woman after Catherine Duncan to be given a directorial role in the unit, making a series of short documentaries. After a 10-year hiatus raising her children, she worked on two important historical documentaries on the Australian feature film industry – The Pictures That Moved: Australian Cinema 1896–1920 (1968) and The Passionate Industry 1920–1930 (1973) – which established her significant role as a writer, producer and director. She went on to pursue a career in feature films, including as scriptwriter for Caddie (1976) and producer of Puberty Blues (1981). She became the first woman president of the Australian Writers Guild in 1972 and was awarded an AM for her services to the film industry in 1980.", "biography_author": null, "date_of_biography": "2023-08-09T03:55:52.438963+10:00", "external_links": [], "uuid": "71d164e1-df1d-4d8b-83d7-504a91bad8a8", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "91", "external_references": [{"id": 26068, "source": {"name": "Wikidata", "slug": "wikidata"}, "source_identifier": "Q6205234"}], "date_modified": "2023-08-09T03:55:52.439277+10:00"}, {"id": 10014, "name": "Stan Brakhage", "roles_in_work": [{"id": 2403292, "title": "By Brakhage: an anthology", "work_id": 94577, "role": "director", "role_id": 938, "is_primary": true}], "also_known_as": "", "date_of_birth": "1933", "date_of_death": "2003", "genders": [{"id": 9, "name": "Male", "slug": "male"}], "languages": [], "places_of_operation": [], "biography": "A celebrated figure of the international avant-garde, Stan Brakhage filmed what he termed the 'adventures' of visual perception.  From his famed Sirius Remembered (1959), Mothlight (1963) and Dog Star Man (1961-64) to the anti-narrative, visual explorations of form and structure in The Art of Vision (1965) and The Text of Light (1974), Brakhage filmed not just the world but the act of seeing the world.  Questioning the relationship between moving image representation and the world, Brakhage strove to make the viewer aware of vision as a dynamic, nerve-centered event.\n\nAmong the first filmmakers to alter the filmstrip for expressive effect, he scratched, painted, dyed and overlayed patterns directly onto the negative right up until his very last film, the unfinished Chinese Series\n(2003).  After his death in 2003, the Museum of Modern Art in New York is now in the process of preserving all of Brakhage's 400-odd films for their permanent collection.\n\n(PROOF exhibition bio 2004) SW", "biography_author": null, "date_of_biography": "2023-08-11T15:09:10.633362+10:00", "external_links": [], "uuid": "202d4aea-f0fa-49cd-be77-66b303ab33da", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "110651", "external_references": [], "date_modified": "2023-08-11T15:09:10.633588+10:00"}]}