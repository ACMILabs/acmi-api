_of_operation": [], "biography": "", "biography_author": null, "date_of_biography": "2023-05-09T19:28:28.407822+10:00", "external_links": [], "uuid": "1556e062-6288-4467-b136-fc0e4fbb77f1", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "1687", "external_references": [], "date_modified": "2023-06-14T16:33:30.576250+10:00"}, {"id": 9268, "name": "Deborah Kelly", "roles_in_work": [{"id": 2351458, "title": "The Gods of Tiny Things", "work_id": 119198, "role": "artist", "role_id": 505, "is_primary": true}, {"id": 730463, "title": "Boat people", "work_id": 109231, "role": "creator", "role_id": 968, "is_primary": true}], "also_known_as": "", "date_of_birth": "", "date_of_death": "", "places_of_operation": [], "biography": "Deborah Kelly\nb. 1962 Melbourne, Victoria\nLives mainly in Sydney & part time in Melbourne, works everywhere she can.\n\nDeborah Kelly has been making socially engaged artwork since 1983. She was also a Creative Director of Australia's largest public interest communications company for 7 years. Her projects implicate broad audiences in the discourses of local & momentary histories, and seek to intervene into representation of place and privilege. Recent collaborative projects have included boat-people.org, graphic civil disobedience on refugees, mass actions on Aboriginal Land Rights, and the prize-winning public art project with Tina Fiveash \"Hey, hetero!\", which has been shown in Sydney, Adelaide, Melbourne and Wellington. She co-curated and conceived the exhibition / symposium / tactical media lab BORDERPANIC, a Performance Space/Museum of Contemporary Art/Next 5Minutes co-production in September 2002, with Zina Kaye. She has given public lectures and workshops around Australia and in San Francisco, Los Angeles and Ottawa. She was invited by Martha Rosler to a political artists' residency in Florida, USA, Jan- Feb 2002, and remains actively engaged in online collaborations with that crew.\n\nCurrent projects include being commissioning editor/artist for It's Great to Be Straight: a critical anthology regarding heterosexuality for Pluto Press (forthcoming), boat people.org, TPV holder's legal project with Smash Racism, and working with Martha Rosler on a collaborative installation and poster series for the Utopia Station at the Venice Bienalle, 2003.\n\ninfo from nonacmi source.", "biography_author": null, "date_of_biography": "2023-05-09T19:56:48.176991+10:00", "external_links": [], "uuid": "4e096640-4ff7-434b-abaa-0d8283217429", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "112248", "external_references": [], "date_modified": "2023-06-14T16:33:30.576250+10:00"}, {"id": 9266, "name": "Lisa French", "roles_in_work": [{"id": 2117278, "title": "Birth of a film festival", "work_id": 109230, "role": "producer", "role_id": 1089, "is_primary": true}], "also_known_as": "", "date_of_birth": "", "date_of_death": "", "places_of_operation": [], "biography": "", "biography_author": null, "date_of_biography": "2023-05-09T12:55:59.137069+10:00", "external_links": [], "uuid": "9f8f6739-e7f0-479a-a4ba-9ebacc082f14", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "112239", "external_references": [{"id": 39257, "source": {"name": "Wikidata", "slug": "wikidata"}, "source_identifier": "Q121126083"}], "date_modified": "2023-06-14T16:33:30.576250+10:00"}, {"id": 9265, "name": "Mark Poole", "roles_in_work": [{"id": 2117274, "title": "Birth of a film festival", "work_id": 109230, "role": "director", "role_id": 938, "is_primary": true}, {"id": 1882802, "title": "Energy management in commercial buildings", "work_id": 115493, "role": "director", "role_id": 938, "is_primary": true}, {"id": 1882798, "title": "Energy efficient air", "work_id": 87129, "role": "director", "role_id": 938, "is_primary": true}, {"id": 1882794, "title": "The Energy audit", "work_id": 87128, "role": "director", "role_id": 938, "is_primary": true}, {"id": 1882790, "title": "The Energy management 