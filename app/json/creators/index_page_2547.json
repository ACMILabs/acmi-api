ces_of_operation": [{"id": 3, "name": "Australia", "slug": "australia"}], "biography": "David Parer ACS is an Australian natural history film maker, working in partnership with his wife and sound recordist, Elizabeth Parer-Cook.Parer was conscripted into the Australian Army to go to the Vietnam War in 1970, but he entered a Masters program to study physics in the Antarctic. Parer spent the summers of 1970 and 1972 in Antarctica studying cosmic rays at Mawson Station. While there he filmed his first documentary. David subsequently joined the Australian Broadcasting Corporation ABC Natural History Unit making wildlife films, where he met his wife and fellow film maker, Elizabeth Parer-Cook, in 1977. After the Natural History Unit closed in 2007, the Parers have continued working as a freelance team.", "biography_author": "Wikidata", "date_of_biography": "2023-09-05T19:01:49.890578+10:00", "external_links": [{"id": 5769, "uri": "https://en.wikipedia.org/wiki/David_Parer"}], "uuid": "dca44576-18d3-43c5-8285-cb3f4db39ae5", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "17746", "external_references": [{"id": 51804, "source": {"name": "Wikidata", "slug": "wikidata"}, "source_identifier": "Q5238371"}], "date_modified": "2025-03-06T23:17:23.388223+11:00"}, {"id": 12501, "name": "Dept of Science Aust", "roles_in_work": [{"id": 2677341, "title": "Antarctic summer", "work_id": 64816, "role": "production company", "role_id": 941, "is_primary": true}], "also_known_as": "", "date_of_birth": "", "date_of_death": "", "genders": [], "languages": [], "places_of_operation": [], "biography": "", "biography_author": null, "date_of_biography": "2023-05-09T16:54:45.068842+10:00", "external_links": [], "uuid": "5dbfa2ad-1f39-45c0-9288-610261c1697c", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "7514", "external_references": [{"id": 39219, "source": {"name": "Wikidata", "slug": "wikidata"}, "source_identifier": "Q121271163"}], "date_modified": "