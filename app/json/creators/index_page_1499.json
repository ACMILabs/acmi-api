f_operation": [], "biography": "", "biography_author": null, "date_of_biography": null, "external_links": [], "uuid": "fe6a8ed2-8c66-4bb1-b1b3-38c5071984f6", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "14905", "external_references": [{"id": 6972, "source": {"name": "Wikidata", "slug": "wikidata"}, "source_identifier": "Q4989693"}, {"id": 6973, "source": {"name": "VIAF", "slug": "viaf"}, "source_identifier": "44209830"}, {"id": 6974, "source": {"name": "LOC Auth", "slug": "loc-auth"}, "source_identifier": "nr98005206"}, {"id": 6975, "source": {"name": "WorldCat", "slug": "worldcat"}, "source_identifier": "lccn-nr98005206"}], "date_modified": "2023-09-13T12:28:05.359704+10:00"}, {"id": 33552, "name": "Roy Andersson", "roles_in_work": [{"id": 1995866, "title": "Songs from the second floor", "work_id": 92355, "role": "director", "role_id": 938, "is_primary": true}], "also_known_as": "Roy Arne Lennart Andersson", "date_of_birth": "1943-03-31", "date_of_death": null, "genders": [{"id": 9, "name": "Male", "slug": "male"}], "languages": [{"id": 35, "name": "Swedish", "slug": "swedish"}], "places_of_operation": [{"id": 43, "name": "Sweden", "slug": "sweden"}], "biography": "Roy Arne Lennart Andersson (born 31 March 1943) is a Swedish film director, best known for A Swedish Love Story (1970), About Endlessness (2019) and his \"Living trilogy,\" which includes Songs from the Second Floor (2000), You, the Living (2007) and A Pigeon Sat on a Branch Reflecting on Existence (2014). Songs from the Second Floor, more than any other, cemented and exemplified his personal style – which is characterized by long takes, absurdist comedy, stiff caricaturing  of Swedish culture and grotesque. He has spent much of his professional life working on advertisement spots, directing over 400 commercials and two short films; directing six feature-length films in six decades. His 2014 film A Pigeon Sat on a Branch Reflecting on Existence won the Golden Lion award at 71st Venice International Film Festival, making Andersson the only Swedish director and the second Nordic director to win the award in the history of the festival, after Danish Carl Theodor Dreyer won in 1955. Andersson is considered one of the most important living European film directors, having four films officially submitted for the Academy Award for Best Foreign Language Film as Swedish entries.", "biography_author": "Wikidata", "date_of_biography": "2023-08-01T16:56:08+10:00", "external_links": [{"id": 5085, "uri": "https://en.wikipedia.org/wiki/Roy_Andersson"}], "uuid": "7aa480a9-6fa7-425f-9c22-bc53e9278f67", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "28295", "external_references": [{"id": 25827, "source": {"name": "Wikidata", "slug": "wikidata"}, "source_identifier": "Q703513"}, {"id": 31326, "source": {"name": "VIAF", "slug": "viaf"}, "source_identifier": "85449282"}, {"id": 31327, "source": {"name": "LOC Auth", "slug": "loc-auth"}, "source_identifier": "n96106655"}, {"id": 31328, "source": {"name": "WorldCat", "slug": "worldcat"}, "source_identifier": "lccn-n96106655"}], "date_modified": "2023-09-13T15:06:25.862075+10:00"}]}