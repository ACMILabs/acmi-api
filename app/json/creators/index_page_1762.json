st managed, and in some cases developed, by Chet Helms.", "biography_author": "Wikidata", "date_of_biography": "2022-05-19T10:57:05.031737+10:00", "external_links": [{"id": 1728, "uri": "https://en.wikipedia.org/wiki/Bill_Graham_(promoter)"}], "uuid": "46cc2eea-942e-407d-8f57-097bd878bfa1", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "5854", "external_references": [{"id": 7534, "source": {"name": "Wikidata", "slug": "wikidata"}, "source_identifier": "Q862152"}, {"id": 7535, "source": {"name": "VIAF", "slug": "viaf"}, "source_identifier": "47564040"}, {"id": 7536, "source": {"name": "LOC Auth", "slug": "loc-auth"}, "source_identifier": "n91015400"}, {"id": 7537, "source": {"name": "WorldCat", "slug": "worldcat"}, "source_identifier": "lccn-n91015400"}], "date_modified": "2025-02-26T20:21:34.065927+11:00"}, {"id": 27081, "name": "Sasha Harari", "roles_in_work": [{"id": 2615274, "title": "The Doors", "work_id": 85174, "role": "co-producer", "role_id": 915, "is_primary": true}, {"id": 2615271, "title": "The Doors", "work_id": 85174, "role": "production company", "role_id": 941, "is_primary": true}], "also_known_as": "", "date_of_birth": "", "date_of_death": "", "genders": [], "languages": [], "places_of_operation": [], "biography": "", "biography_author": null, "date_of_biography": "2023-05-09T19:44:44.750071+10:00", "external_links": [], "uuid": "e2b387ea-8a5e-457e-8b5c-17695e1a4154", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "28597", "external_references": [{"id": 36807, "source": {"name": "Wikidata", "slug": "wikidata"}, "source_identifier": "Q121126368"}], "date_modified": "2025-02-26T20:21:35.229543+11:00"}, {"id": 27078, "name": "France 3 Cinema", "roles_in_work": [{"id": 1998012, "title": "La Ceremonie", "work_id": 93584, "role": "production company", "role_id": 941, "is_primary": true}, {"id": 1995621, "title": "Alice and Martin = Alice et Martin", "work_id": 91875, "role": "production company", "role_id": 941,