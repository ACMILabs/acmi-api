ternal_links": [], "uuid": "aaf24fa2-d8d1-42c6-a0a1-71b3a73896c9", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": null, "external_references": [], "date_modified": "2023-06-14T16:33:30.576250+10:00"}, {"id": 79544, "name": "Neal Edelstein", "roles_in_work": [{"id": 1998100, "title": "Mulholland Drive", "work_id": 93609, "role": "co-producer", "role_id": 915, "is_primary": true}, {"id": 1995715, "title": "Lumiere and company", "work_id": 92216, "role": "co-producer", "role_id": 915, "is_primary": true}, {"id": 1995269, "title": "The Straight story", "work_id": 91314, "role": "co-producer", "role_id": 915, "is_primary": true}], "also_known_as": "", "date_of_birth": null, "date_of_death": null, "places_of_operation": [{"id": 1, "name": "United States of America", "slug": "united-states-of-america"}], "biography": "Neal Edelstein is an independent film producer and is known as an early technology adopter.", "biography_author": "Wikidata", "date_of_biography": "2023-08-01T16:21:59.953535+10:00", "external_links": [{"id": 4765, "uri": "https://en.wikipedia.org/wiki/Neal_Edelstein"}], "uuid": "9fb7eca9-4186-4a75-9c80-c3c3f998c277", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": null, "external_references": [{"id": 25056, "source": {"name": "Wikidata", "slug": "wikidata"}, "source_identifier": "Q16249543"}], "date_modified": "2023-08-01T16:21:59.982337+10:00"}, {"id": 79543, "name": "Tim Roth", "roles_in_work": [{"id": 910644, "title": "The War zone (E)", "work_id": 91312, "role": "director", "role_id": 938, "is_primary": true}], "also_known_as": "Timothy Simon Roth", "date_of_birth": "1961-05-14", "date_of_death": null, "places_of_operation": [{"id": 10, "name": "United Kingdom", "slug": "united-kingdom"}], "biography": "Timothy Simon Roth (; born 14 May 1961) is an English actor and producer. He began acting on films and television series in the 1980s. He was among a group of prominent British actors of the era, the \"Brit Pack\". \nHe made his television debut in Made in Britain (1982), and his film debut in The Hit (1984), for which he was nominated for the BAFTA Award for Most Promising Newcomer. Since then, he gained more attention for his roles in films, including The Cook, the Thief, His Wife & Her Lover, (1989), Vincent & Theo (1990), and Rosencrantz & Guildenstern Are Dead (1990). Roth collaborated with Quentin Tarantino on several films, such as Reservoir Dogs (1992), Pulp Fiction (1994), Four Rooms (1995),  Hoodlum (1997), and The Hateful Eight (2015). For his performance in Rob Roy (1995), Roth won the BAFTA Award for Best Actor in a Supporting Role, and was nominated for a Golden Globe Award for Best Supporting Actor – Motion Picture and the Academy Award for Best Supporting Actor. \nRoth made his directorial debut with the film The War Zone (1999). He played Cal Lightman in the Fox series Lie to Me (2009–2011) and Jim Worth / Jack Devlin in the Sky Atlantic series Tin Star (2017–2020). Roth played Emil Blonsky / Abomination in the Marvel Cinematic Universe film The Incredible Hulk (2008), and reprised his role in Shang-Chi and the Legend of the Ten Rings (2021), and the Disney+ series She-Hulk: Attorney at Law (2022).", "biography_author": "Wikidata", "date_of_biography": "2023-08-01T17:18:29.194110+10:00", "external_links": [{"id": 5267, "uri": "https://en.wikipedia.org/wiki/Tim_Roth"}], "uuid": "52e96210-efe8-4657-9f70-9c2a1c4fb52e", "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": null, "external_references": [{"id": 25740, "source": {"name": "Wikidata", "slug": "wikidata"}, "source_identifier": "Q203804"}, {"id": 31796, "source": {"name": "VIAF", "slug": "viaf"}, "source_identifier": "85081586"}, {"id": 31797, "source": {"name": "LOC Auth", "slug": "loc-auth"}, "source_identifier": "no95048300"}, {"id": 31798, "source": {"name": "WorldCat", "slug": "worldcat"}, "source_identifier": "lccn-no95048300"}], "date_modified": "2023-08-01T17:18:36.125214+10:00"