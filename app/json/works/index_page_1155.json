ll}, "group_works": [], "group_siblings": [{"id": 112055, "acmi_id": "183507", "title": "Small square cedar table", "title_annotation": "", "slug": "small-square-cedar-table", "creator_credit": "", "credit_line": "", "headline_credit": "", "thumbnail": null, "record_type": "work", "type": "Object", "is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2031-02-16", "is_context_indigenous": false, "material_description": "cedar wood", "unpublished": true, "first_production_date": null}], "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "184333", "production_places": [], "production_dates": [], "labels": [57804]}, {"id": 117969, "acmi_id": "183942", "title": "Wilhelm scream", "title_annotation": "", "slug": "wilhelm-scream-exhibition-video", "creator_credit": "", "credit_line": "", "headline_credit": "", "thumbnail": {"image_url": "https://acmi-public-api.s3.amazonaws.com/image/183942_WIlhelmScreenSupercut_DistantDrums_still.jpg.1200x1200_q85.jpg", "has_video": false}, "record_type": "work", "type": "Film", "is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2031-02-16", "is_context_indigenous": false, "material_description": "", "unpublished": false, "first_production_date": null, "brief_description": "<p>Exhibition video created by ACMI for the Fed Square Foyer display; forefronting the use of the &#8216;Wilhelm Scream&#8217; in film and TV</p>", "constellations_primary": [{"id": 314, "name": "The Wilhelm scream, easters eggs and open world games", "description": "This Constellation tracks how an on-set in-joke became one of the first cinematic easter eggs. It was made by Swinburne Cinema and Screen Studies students Harrison Buikstra and Jasper Caverly."}], "constellations_other": [], "recommendations": [], "title_for_label": "<p>Wilhelm scream</p>", "creator_credit_for_label": "", "headline_credit_for_label": "", "description": "<p>First appearing in <i>Distant Drums</i> (1951) to express the terror of being eaten by alligators, the Wilhelm scream earned its namesake in 1953, when Private Wilhelm was shot in the leg with an arrow in <i>The Charge at Feather River</i>. In the 1970s, University of Southern California film students found the stock sound effect and used it in their student films. One of those students was Ben Burtt, who became sound editor on both the <i>Star Wars</i> and <i>Indiana Jones</i> franchises, and used the effect throughout each series. Other filmmakers noticed and incorporated it into their own sound design as a running industry in-joke. It’s since appeared in <i>Titanic, Reservoir Dogs</i> and <i>Toy Story</i>, as well as countless other films, TV shows and videos. Today, many people encounter the Wilhelm scream via memes, demonstrating how different forms of pop culture influence and inspire each other.</p>", "description_for_label": ["<p>First appearing in <i>Distant Drums</i> (1951) to express the terror of being eaten by alligators, the Wilhelm scream earned its namesake in 1953, when Private Wilhelm was shot in the leg with an arrow in <i>The Charge at Feather River</i>. In the 1970s, University of Southern California film students found the stock sound effect and used it in their student films. One of those students was Ben Burtt, who became sound editor on both the <i>Star Wars</i> and <i>Indiana Jones</i> franchises, and used the effect throughout each series. Other filmmakers noticed and incorporated it into their own sound design as a running industry in-joke. It’s since appeared in <i>Titanic, Reservoir Dogs</i> and <i>Toy Story</i>, as well as countless other films, TV shows and videos. Today, many people encounter the Wilhelm scream via memes, demonstrating how different forms of pop culture influence and inspire each other.</p>"], "credit_line_for_label": "", "details": [{"label": "Curatorial section", "display_values": ["The Story of the Moving Image&nbsp;→  Foyer&nbsp;→ Entry &nbsp;→ 02. Fed Sq Foyer Cabinets"]}], "stats": {"tap_count": 83}, "links": [], "creators_primary": [], "creators_other": [], "video_links": [], "media_note": null, "images": [{"id": 17153, "image_file": "https://acmi-public-api.s3.amazonaws.com/image/183942_WIlhelmScreenSupercut_DistantDrums_still.jpg", "image_file_thumbnail": "https://acmi-public-api.s3.amazonaws.com/image/183942_WIlhelmScreenSupercut_DistantDrums_still.jpg.1200x1200_q85.jpg", "image_file_xs": "https://acmi-public-api.s3.amazonaws.com/image/183942_WIlhelmScreenSupercut_DistantDrums_still.jpg.480x480_q85.jpg", "image_file_s": "https://acmi-public-api.s3.amazonaws.com/image/183942_WIlhelmScreenSupercut_DistantDrums_still.jpg.960x960_q85.jpg", "image_file_m": "https://acmi-public-api.s3.amazonaws.com/image/183942_WIlhelmScreenSupercut_DistantDrums_still.jpg.1920x1920_q85.jpg", "image_file_l": "https://acmi-public-api.s3.amazonaws.com/image/183942_WIlhelmScreenSupercut_DistantDrums_still.jpg.3840x3840_q85.jpg", "caption": "Distant Drums (still)", "credit_line": "A still from Distant Drums (1951)", "alt_text": "", "width": 1280, "height": 720, "access_level": "View anywhere"}], "videos": [], "holdings": [], "part_of": null, "parts": [], "part_siblings": [], "group": null, "group_works": [], "group_siblings": [], "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "183942", "production_places": [], "production_dates": [], "labels": [57464]}]}