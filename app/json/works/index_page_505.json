as_environment_id": null, "production_places": [{"id": 276, "name": "Syria", "slug": "syria"}], "production_dates": [{"date": "2019", "notes": "", "to_year": "2019"}], "labels": [47048], "external_references": []}, {"id": 100581, "acmi_id": "", "title": "The Story of the Kelly Gang", "title_annotation": "", "slug": "the-story-of-the-kelly-gang", "creator_credit": "Charles Tait", "credit_line": "<p>Courtesy the National Film and Sound Archive of Australia.</p>", "headline_credit": "Australia, 1906", "video_links": [], "record_type": "group", "type": "Film", "is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2031-02-16", "is_context_indigenous": false, "material_description": "", "unpublished": false, "external": false, "first_production_date": "1906-01-01T10:00:00+10:00", "commissioned": false, "brief_description": "<p>Produced in Melbourne in 1906, The Story of the Kelly Gang is the world’s first feature-length film. While most movies of its era only ran for about 12 minutes, Charles Tait’s bushranger epic went for over an hour.</p>", "constellations_primary": [{"id": 85, "name": "Outlawed", "description": "From <i>The Story of the Kelly Gang</i> to <I>Sweet Country</i>, this Constellation explores bushrangers, samurai and bandits."}], "constellations_other": [{"id": 137, "name": "Crime doesn't pay", "description": "Ned Kelly's famous armour has appeared in many films, but the criminal myth speaks to how audiences have always been captivated by true-crime stories, from the beginning of cinema to today."}, {"id": 218, "name": "Bell & Howell Projector 621: This picture has been edited for content", "description": "The Bell & Howell-Gaumont Model 621 projector was originally used entertain the shearers on a sheep farm in South Australia with the films screened sometimes edited for distribution."}, {"id": 317, "name": "True blue, true crime", "description": "Robert Connolly's <i>The Dry</i> is a recent Australian crime blockbuster, but the writer, director and producer's first film (and first crime film) connects to a longstanding tradition of true-blue crime stories from some of our greatest filmmakers."}], "recommendations": [], "title_for_label": "<p>The Story of the Kelly Gang</p>", "creator_credit_for_label": "<p>Charles Tait</p>", "headline_credit_for_label": "<p>Australia, 1906</p>", "description": "<p>Produced in Melbourne in 1906, <i>The Story of the Kelly Gang</i> is the world’s first feature-length film. While most movies of its era only ran for about 12 minutes, Charles Tait’s bushranger epic went for over an hour. But it wasn’t just the longest film by time: it was literally ‘the longest film ever made’, at a staggering 4,000 feet of celluloid.</p>\n<p>Its popularity was also staggering, so much so that in 1914 the New South Wales government banned the production of bushranger films, believing that <i>The Story of the Kelly Gang</i> influenced rising crime. That’s debatable, but the film’s influence on other filmmakers isn’t.</p>", "description_for_label": ["<p>Produced in Melbourne in 1906, <i>The Story of the Kelly Gang</i> is the world’s first feature-length film. While most movies of its era only ran for about 12 minutes, Charles Tait’s bushranger epic went for over an hour. But it wasn’t just the longest film by time: it was literally ‘the longest film ever made’, at a staggering 4,000 feet of celluloid.</p>\n<p>Its popularity was also staggering, so much so that in 1914 the New South Wales government banned the production of bushranger films, believing that <i>The Story of the Kelly Gang</i> influenced rising crime. That’s debatable, but the film’s influence on other filmmakers isn’t.</p>"], "credit_line_for_label": "<p>Courtesy the National Film and Sound Archive of Australia.</p>", "details": [{"label": "Curatorial section", "display_values": ["The Story of the Moving Image&nbsp;→ Moving Pictures&nbsp;→ MI-04. Materiality&nbsp;→ MI-04-C01"]}], "stats": {"tap_count": 20272}, "links": [], "creators_primary": [{"id": 1857834, "name": "Charles Tait", "creator_id": 49339, "creator_wikidata_id": null, "role": "creator", "role_id": 968, "is_primary": true}], "creators_other": [], "media_note": null, "holdings": [], "part_of": null, "parts": [], "part_siblings": [], "group": null, "group_works": [{"id": 99819, "acmi_id": "P180329", "title": "Frank Mills as Ned Kelly in Story of the Kelly Gang", "title_annotation": "facsimile", "slug": "graphic-frank-mills-as-ned-kelly-the-story-of-the-kelly-gang-1906", "creator_credit": "", "credit_line": "<p>Courtesy GL Archive / Alamy</p>", "headline_credit": "", "video_links": [], "record_type": "work", "type": "Object", "is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2031-02-16", "is_context_indigenous": false, "material_description": "graphic", "unpublished": false, "external": false, "first_production_date": null, "commissioned": false}, {"id": 99820, "acmi_id": "P180330", "title": "Death of Dan Kelly and Steve Hart in Story of the Kelly Gang", "title_annotation": "Facsimile", "slug": "graphic-death-of-dan-kelly-and-steve-hart-the-story-of-the-kelly-gang-1906", "creator_credit": "", "credit_line": "<p>Courtesy World History Archive / Alamy</p>", "headline_credit": "Australia, 1906", "video_links": [], "record_type": "work", "type": "Object", "is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2031-02-16", "is_context_indigenous": false, "material_description": "graphic", "unpublished": false, "external": false, "first_production_date": "1906-01-01T10:00:00+10:00", "commissioned": false}, {"id": 99691, "acmi_id": "FC180162", "title": "The Story of the Kelly Gang (1906) movie poster", "title_annotation": "Facsimile", "slug": "facsimile-film-poster-the-story-of-the-kelly-gang-1906", "creator_credit": "", "credit_line": "<p>Courtesy the National Film and Sound Archive of Australia.</p>", "headline_credit": "1906", "video