_id": null, "production_places": [{"id": 2, "name": "United States", "slug": "united-states"}], "production_dates": [{"date": "2019", "notes": "", "to_year": "2019"}], "labels": [61579], "external_references": []}, {"id": 122119, "acmi_id": "P191201", "title": "Hollywood Lesbians", "title_annotation": "", "slug": "hollywood-lesbians", "creator_credit": "Boze Hadleigh", "credit_line": "", "headline_credit": "United States, 1994", "video_links": [], "record_type": "work", "type": "Object", "is_on_display": true, "last_on_display_place": "Australian Centre for the Moving Image", "last_on_display_date": "2023-10-01", "is_context_indigenous": false, "material_description": "hardcover book", "unpublished": false, "external": false, "first_production_date": "1994-01-01T11:00:00+11:00", "commissioned": false, "brief_description": "<p>Hollywood Lesbians is a collection of interviews with 10 lesbians in the entertainment industry. Publishers Weekly wrote: &#8220;Fans of Hollywood&#8217;s golden age will find this collection of interviews conducted over many years revealing, though hampered. The subjects - director Dorothy Arzner, designer Edith Head, actresses Judith Anderson, Marjorie Main, Barbara Stanwyck, Nancy Kulp, Capucine, Patsy Kelly, Agnes Moorehead and Sandy Dennis — were raised in a generation terrified of voicing support for fellow homosexuals, let alone daring to come out of the closet to acknowledge their own sexuality&#8230; Still, with carefully couched questions from Hadleigh, though some of the respondents dance around the subjects of sex and sexuality. Still, an enlightening picture emerges of Tinseltown, different from that presented in the fanzines.&#8221; (from Wikipedia)</p>", "constellations_primary": [], "constellations_other": [], "recommendations": [], "title_for_label": "<p>Hollywood Lesbians</p>", "creator_credit_for_label": "<p>Boze Hadleigh</p>", "headline_credit_for_label": "<p>United States, 1994</p>", "description": "<p>Hollywood Lesbians is