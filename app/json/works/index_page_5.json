ings": [], "part_of": null, "parts": [], "part_siblings": [], "group": {"id": 112116, "acmi_id": "", "title": "Character concept art with feedback from Jacob Nash", "title_annotation": "", "slug": "character-concept-art-with-feedback-from-jacob-nash", "creator_credit": "Weta Workshop", "credit_line": "<p>Courtesy Goalpost Pictures</p>", "headline_credit": "", "video_links": [], "record_type": "group", "type": "Film", "is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2024-11-10", "is_context_indigenous": false, "material_description": "", "unpublished": false, "external": true, "first_production_date": null, "commissioned": false, "external_references": []}, "group_works": [], "group_siblings": [], "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "180551", "eaas_environment_id": null, "production_places": [{"id": 3, "name": "Australia", "slug": "australia"}], "production_dates": [{"date": "2016", "notes": "", "to_year": "2016"}], "labels": [40236]}, {"id": 107853, "acmi_id": "183481", "title": "VFX in Peter Rabbit", "title_annotation": "", "slug": "vfx-in-peter-rabbit", "creator_credit": "Will Gluck", "credit_line": "<p>PR1: PETER RABBIT and all associated characters ™ &amp; © Frederick Warne &amp; Co Limited. PETER RABBIT™, the Movie ©2018 Columbia Pictures Industries, Inc. All Rights Reserved.<br />\nPR2: PETER RABBIT and all associated characters ™ &amp; © Frederick Warne &amp; Co Limited. PETER RABBIT™ 2:The Runaway, the Movie ©2021 Columbia Pictures Industries, Inc. All Rights Reserved.</p>", "headline_credit": "United States, 2018", "video_links": [], "record_type": "work", "type": "Film", "is_on_display": false, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2022-02-11", "is_context_indigenous": false, "material_description": "", "unpublished": false, "external": false, "first_production_date": "2018-01-01T11:00:00+11:00", "commissioned": false, "external_references": [], "brief_description": "<p>A look at how acclaimed VFX studio Animal Logic bought Beatrix Potter&#8217;s Peter Rabbit to life.</p>", "constellations_primary": [{"id": 96, "name": "VFX to fantasy and freedom", "description": "Founded in Sydney, and now with offices worldwide, Animal Logic are an Academy Award-winning VFX and animation studio whose job is to bring the impossible to life. And they're in good company with these other artists and filmmakers."}], "constellations_other": [], "recommendations": [], "title_for_label": "<p>Peter Rabbit</p>", "creator_credit_for_label": "<p>Will Gluck</p>", "headline_credit_for_label": "<p>United States, 2018</p>", "description": "<p>First springing from the mind of Beatrix Potter, Peter Rabbit is a creature that has captured the imagination of children for more than a century. So there was plenty of pressure when it came to bringing the beloved character to life on the big screen. Animal Logic rose to the occasion, with the Australian company having to get inventive. To achieve a seamless hybrid of live action and animation for <i>Peter Rabbit</i> (2018), they used prop rabbits during the shoot so cast members had a tangible property to interact with. In post-production, the Prop Peters were replaced with an animated version by visual effect artists.</p>", "description_for_label": ["<p>Beatrix Potter’s Peter Rabbit has captured the imagination of children for more than a century. So there was plenty of pressure when it came to bringing the character to life on the big screen. Animal Logic rose to the occasion, with the Australian company having to get inventive. To achieve a seamless hybrid of live-action and animation for <i>Peter Rabbit</i>, they used prop rabbits during the shoot for cast members to interact with. In post-production, the Prop Peters were replaced with an animated version by visual effect artists.</p>"], "credit_line_for_label": "<p>PETER RABBIT™ &amp; © FW&amp;Co. PETER RABBIT™ Movie © 2021 CPII All Rights Reserv