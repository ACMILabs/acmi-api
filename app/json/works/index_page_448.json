is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2031-02-16", "is_context_indigenous": false, "material_description": "", "unpublished": false, "first_production_date": "2016-01-01T11:00:00+11:00", "brief_description": "<p>Safeguard society by spying on your fellow citizens in this game based on the themes of George Orwell’s novel <I>1984</i> (1949).</p>", "constellations_primary": [{"id": 100, "name": "Orwell & Snowden", "description": "Explores Orwell as a representation of the NSA, and the infamous Snowden leak."}], "constellations_other": [], "recommendations": [], "title_for_label": "<p>Orwell: Keeping an Eye on You</p>", "creator_credit_for_label": "<p>Fellow Traveller and Osmotic Studios</p>", "headline_credit_for_label": "<p>Australia and Germany, 2016</p>", "description": "<p>Safeguard society by spying on your fellow citizens in this game based on the themes of George Orwell’s novel <I>1984</i> (1949). Transforming the dystopian thriller into a click-and-play game set in our contemporary online world, players are cast as government operatives who trawl social media profiles to uncover terrorists and political dissidents. <I>Orwell: Keeping an Eye on You</i> presents players with moral dilemmas, forcing them to think about their own ethics and beliefs when responding to the game’s reflection of our current society. It also asks them to analyse the effect of surveillance culture on society and the proliferation of false information and fake news.</p>", "description_for_label": ["<p>Safeguard society by spying on your fellow citizens in this game based on the themes of George Orwell’s novel <I>1984</i> (1949). Transforming the dystopian thriller into a click-and-play game set in our contemporary online world, players are cast as government operatives who trawl social media profiles to uncover terrorists and political dissidents. <I>Orwell: Keeping an Eye on You</i> presents players with moral dilemmas, forcing them to think about their own ethics and beliefs when responding to the game’s reflection of our current society. It also asks them to analyse the effect of surveillance culture on society and the proliferation of false information and fake news.</p>"], "credit_line_for_label": "", "details": [{"label": "Curatorial section", "display_values": ["The Story of the Moving Image&nbsp;→ Games Lab&nbsp;→ GL-03. Cluster 3&nbsp;→ GL-03-C06"]}], "stats": {"tap_count": 6216}, "links": [], "creators_primary": [{"id": 1556121, "name": "Fellow Traveller", "creator_id": 84335, "creator_wikidata_id": null, "role": "creator", "role_id": 968, "is_primary": true}, {"id": 1556120, "name": "Osmotic Studios", "creator_id": 84334, "creator_wikidata_id": null, "role": "creator", "role_id": 968, "is_primary": true}], "creators_other": [], "video_links": [], "media_note": null, "holdings": [], "part_of": null, "parts": [], "part_siblings": [], "group": null, "group_works": [{"id": 117929, "acmi_id": "", "title": "Character concept art and game screenshots", "title_annotation": "", "slug": "character-concept-art-and-game-screenshots", "creator_credit": "", "credit_line": "", "headline_credit": "", "record_type": "group", "type": "Film", "is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2031-02-16", "is_context_indigenous": false, "material_description": "", "unpublished": false, "first_production_date": null}], "group_siblings": [], "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "183890", "eaas_environment_id": null, "production_places": [{"id": 3, "name": "Australia", "slug": "australia"}, {"id": 18, "name": "Germany", "slug": "germany"}], "production_dates": [{"date": "2016", "notes": "", "to_year": "2016"}], "labels": [57425]}, {"id": 114893, "acmi_id": "S00530", "title": "Origin of Life", "title_annotation": "", "slug": "origin-of-life", "creator_credit": "", "credit_line": "", "headline_credit": "Unknown", "record_type": "group", "