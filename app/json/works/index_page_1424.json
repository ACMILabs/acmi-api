issioned": false, "public_domain": false, "external_references": [], "brief_description": "<p>Lord Farquaad ruthlessly (and hilariously) interrogates Gingerbread Man in one of the most memorable scenes in Shrek (2001), but before the bit appeared in the movie, storyboard artist Conrad Vernon performed it as a ‘pitch’ to the filmmaking team.</p>", "constellations_primary": [], "constellations_other": [], "recommendations": [], "title_for_label": "<p>Shrek storyboard pitch</p>", "creator_credit_for_label": "", "headline_credit_for_label": "<p>United States, 2015</p>", "description": "<p>Lord Farquaad ruthlessly (and hilariously) interrogates Gingerbread Man in one of the most memorable scenes in <i>Shrek</i> (2001), but before the bit appeared in the movie, storyboard artist Conrad Vernon performed it as a ‘pitch’ to the filmmaking team.</p>\n<p>Animated films aren’t always scripted from start to finish, so teams of artists collaborate on developing scenes and stories they then perform to show how the rhythm and visuals will fit in the completed film.</p>\n<p>Because of the close relationship between story, pacing and drawing, storyboard artists need to be performers and storytellers as well as visual artists. Vernon was definitely a triple threat, and his performance as Gingy in the pitch was so impressive that he voiced the fan-favourite character in the movie.</p>", "description_for_label": ["<p>Lord Farquaad ruthlessly (and hilariously) interrogates Gingerbread Man in one of the most memorable scenes in <i>Shrek</i> (2001), but before the bit appeared in the movie, storyboard artist Conrad Vernon performed it as a ‘pitch’ to the filmmaking team.</p>\n<p>Animated films aren’t always scripted from start to finish, so teams of artists collaborate on developing scenes and stories they then perform to show how the rhythm and visuals will fit in the completed film.</p>\n<p>Because of the close relationship between story, pacing and drawing, storyboard artists need to be performers and storytellers as well as visual artists. Vernon was definitely a triple threat, and his performance as Gingy in the pitch was so impressive that he voiced the fan-favourite character in the movie.</p>"], "credit_line_for_label": "<p>DreamWorks Animation</p>", "details": [{"label": "Curatorial section", "display_values": ["The Story of the Moving Image&nbsp;→ Moving Worlds&nbsp;→ MW-01. Storyboarding&nbsp;→ MW-01-C00"]}, {"label": "Object Types", "display_values": ["Exhibition Prop", "Moving image file/Digital"]}], "stats": {"tap_count": 0}, "links": [], "creators_primary": [], "creators_other": [], "media_note": null, "holdings": [], "part_of": null, "parts": [], "part_siblings": [], "group": {"id": 107818, "acmi_id": "", "title": "The Pitch", "title_annotation": "", "slug": "the-pitch-dreamworks-animation", "creator_credit": "", "credit_line": "<p>Courtesy DreamWorks Animation and Universal Studios</p>", "headline_credit": "United States, 2015", "video_links": [], "record_type": "group", "type": "Video", "is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2025-12-31", "is_context_indigenous": false, "material_description": "", "unpublished": false, "external": true, "first_production_date": "2015-01-01T11:00:00+11:00", "commissioned": false, "public_domain": false, "external_references": []}, "group_works": [], "group_siblings": [], "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "180426", "eaas_environment_id": null, "production_places": [{"id": 2, "name": "United States", "slug": "united-states"}], "production_dates": [{"date": "2015", "notes": "", "to_year": "2015"}], "labels": [40114]}, {"id": 119752, "acmi_id": "B2004295", "title": "Diversity@work - Stories of diversity", "title_annotation": "", "slug": "diversitywork-stories-of-diversity", "creator_credit": "ACMI Digital Storytelling", "credit_line": "", "headline_credit": "Australia, 2004", "video_links": [], "record_type": "group", 