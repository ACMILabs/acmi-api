_id": "", "title": "Headliner: NoviNews", "title_annotation": "", "slug": "headliner-novinews", "creator_credit": "Unbound Creations", "credit_line": "", "headline_credit": "United States, 2018", "video_links": [], "record_type": "group", "type": "Videogame", "is_on_display": false, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2022-02-11", "is_context_indigenous": false, "material_description": "", "unpublished": false, "external": false, "first_production_date": "2018-01-01T11:00:00+11:00", "commissioned": false, "external_references": [], "brief_description": "<p>Unbound Creations’ <i>Headliner: NoviNews</i> explores the impact of the media and fake news on society.</p>", "constellations_primary": [{"id": 292, "name": "Fake news: too little too late", "description": "Unbound Creations’ <i>Headliner: NoviNews</i> explores the impact of the media and fake news on society, something that is fast becoming one of the biggest concerns of our age."}], "constellations_other": [], "recommendations": [], "title_for_label": "<p>Headliner: NoviNews</p>", "creator_credit_for_label": "<p>Unbound Creations</p>", "headline_credit_for_label": "<p>United States, 2018</p>", "description": "<p>Unbound Creations’ <i>Headliner: NoviNews</i> explores the impact of the media and fake news on society. Players take the role of a ‘headliner’, which is like a chief editor at a news outlet. The player must decide what stories and angles to publish, and their choices have wide-reaching effects in the game world. Reject stories about creating national healthcare services? People get sick in the street. Approve stories on the benefits of synthetic recreational drugs? Addiction runs rampant.</p>\n<p>These stories aren’t presented as obviously ‘right’ or ‘wrong’ choices: the game lets the player’s own ethics and morals guide their decisions – which can create a peaceful society or burn it to the ground.</p>", "description_for_label": ["<p>Unbound Creations’ <i>Headliner: NoviNews</i> explores the impact of the media and fake news on society. Players take the role of a ‘headliner’, which is like a chief editor at a news outlet. The player must decide what stories and angles to publish, and their choices have wide-reaching effects in the game world. Reject stories about creating national healthcare services? People get sick in the street. Approve stories on the benefits of synthetic recreational drugs? Addiction runs rampant.</p>\n<p>These stories aren’t presented as obviously ‘right’ or ‘wrong’ choices: the game lets the player’s own ethics and morals guide their decisions – which can create a peaceful society or burn it to the ground.</p>"], "credit_line_for_label": "", "details": [{"label": "Curatorial section", "display_values": ["The Story of the Moving Image&nbsp;→ Games Lab&nbsp;→ GL-03. Cluster 3&nbsp;→ GL-03-C05"]}], "stats": {"tap_count": 29}, "links": [], "creators_primary": [{"id": 1854064, "name": "Unbound Creations", "creator_id": 84332, "creator_wikidata_id": null, "role": "creator", "role_id": 968, "is_primary": true}], "creators_other": [], "media_note": null, "holdings": [], "part_of": null, "parts": [], "part_siblings": [], "group": null, "group_works": [{"id": 117925, "acmi_id": "", "title": "Game screenshots and concept art", "title_annotation": "", "slug": "game-screenshots-and-concept-art", "creator_credit": "", "credit_line": "", "headline_credit": "", "video_links": [], "record_type": "group", "type": "Film", "is_on_display": false, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2022-02-11", "is_context_indigenous": false, "material_description": "", "unpublished": false, "external": false, "first_production_date": null, "commissioned": false, "external_references": []}], "group_siblings": [], "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "183883", "eaas_environment_id": null, "production_places": [{"id": 2, "name": "United States", "slug": "united-states"}], "product