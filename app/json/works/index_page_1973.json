n": "", "unpublished": false, "external": false, "first_production_date": "2014-01-01T11:00:00+11:00", "commissioned": false}, "group_works": [{"id": 107481, "acmi_id": "P180090", "title": "Gone Home deluxe edition box and designer's notebook", "title_annotation": "", "slug": "gone-home-pc-mac-dvd-rom-the-fullbright-company", "creator_credit": "", "credit_line": "", "headline_credit": "", "video_links": [], "record_type": "work", "type": "Videogame", "is_on_display": false, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2022-08-11", "is_context_indigenous": false, "material_description": "", "unpublished": true, "external": false, "first_production_date": null, "commissioned": false}], "group_siblings": [{"id": 117890, "acmi_id": "", "title": "Game screenshots", "title_annotation": "", "slug": "game-screenshots", "creator_credit": "", "credit_line": "", "headline_credit": "", "video_links": [], "record_type": "group", "type": "Film", "is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2031-02-16", "is_context_indigenous": false, "material_description": "", "unpublished": false, "external": false, "first_production_date": null, "commissioned": false}], "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "183849", "eaas_environment_id": null, "production_places": [], "production_dates": [], "labels": [57382], "external_references": []}, {"id": 117889, "acmi_id": "", "title": "Gone Home", "title_annotation": "", "slug": "gone-home", "creator_credit": "Fullbright", "credit_line": "<p>Fullbright, United States, 2013</p>", "headline_credit": "United States, 2014", "video_links": [], "record_type": "group", "type": "Videogame", "is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2031-02-16", "is_context_indigenous": false, "material_description": "", "unpublished": false, "external": false, "first_production_date": "2014-01-01T11:00:00+11:00", "commissioned": false, "brief_description": "<p>When Katie returns home from overseas, she finds her house deserted and her family gone. That’s just one horror trope that <I>Gone Home</i> uses to mislead players as Katie searches the house for her family in this walking simulator.</p>", "constellations_primary": [{"id": 112, "name": "Teens, zines and 90s scenes", "description": "Exploring representations of teen life and culture."}], "constellations_other": [], "recommendations": [], "title_for_label": "<p>Gone Home</p>", "creator_credit_for_label": "<p>Fullbright</p>", "headline_credit_for_label": "<p>United States, 2014</p>", "description": "<p>When Katie returns home from overseas, she finds her house deserted and her family gone.</p>\n<p>That’s just one horror trope that <i>Gone Home</i> uses to mislead players as Katie searches the house for her family. Unlike typical games, <i>Gone Home</i> features minimal activity and was an early ‘walking simulator’, with the story unfolding at the player’s own pace as they examine notes and objects, rather than relying on traditional exposition.</p>\n<p>Developed by indie studio Fullbright, <i>Gone Home</i> was highly praised by critics and fans for its unique style and positive representation of LGBTQIA+ themes.</p>", "description_for_label": ["<p>When Katie returns home from overseas, she finds her house deserted and her family gone.</p>\n<p>That’s just one horror trope that <i>Gone Home</i> uses to mislead players as Katie searches the house for her family. Unlike typical games, <i>Gone Home</i> features minimal activity and was an early ‘walking simulator’, with the story unfolding at the player’s own pace as they examine notes and objects, rather than relying on traditional exposition.</p>\n<p>Developed by indie studio Fullbright, <i>Gone Home</i> was highly praised by critics and fans for its unique style and positive representation of LGBTQIA+ themes.</p>"], "credit_line_for_label": "<p>Fullbright, United States, 2013</p>", "details": [{"label": "Curatorial section", "display_values": ["The Story of the Moving Image&nbsp;→ Games Lab&nbsp;→ GL-01. Cluster 1&nbsp;→ GL-01-C02"]}], "stats": {"tap_count": 21249}, "links": [{"url": "https://store.steampowered.com/app/232430/Gone_Home/", "title": "store.steampowered.com", "source": "vernon"}], "creators_primary": [{"id": 1853941, "name": "Fullbright", "creator_id": 84313, "creator_wikidata_id": null, "role": "creator", "role_id": 968, "is_primary": true}], "creators_other": [], "media_note": null, "holdings": [], "part_of": null, "parts": [], "part_siblings": [], "group": null, "group_works": [{"id": 117890, "acmi_id": "", "title": "Game screenshots", "title_annotation": "", "slug": "game-screenshots", "creator_credit": "", "credit_line": "", "headline_credit": "", "video_links": [], "record_type": "group", "type": "Film", "is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2031-02-16", "is_context_indigenous": false, "material_description": "", "unpublished": false, "external": false, "first_production_date": null, "commissioned": false}, {"id": 117888, "acmi_id": "", "title": "Gone Home deluxe edition and designer's notebook", "title_annotation": "", "slug": "gone-home-deluxe-edition-and-designers-notebook", "creator_credit": "", "credit_line": "", "headline_credit": "", "video_links": [], "record_type": "group", "type": "Film", "is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2031-02-16", "is_context_indigenous": false, "material_description": "", "unpublished": false, "external": false, "first_production_date": null, "commissioned": false}, {"id": 118258, "acmi_id": "P184173", "title": "Gone Home soundtrack", "title_annotation": "", "slug": "gone-home-vinyl-remastered-5th-edition", "creator_credit": "Fullbright", "credit_line": "", "headline_credit": "", "video_links": [], "record_type": "work", "type": "Object", "is_on_display": false, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2022-08-11", "is_context_indigenous": false, "material_description": "", "unpublished": true, "external": false, "first_production_date": null, "commissioned": false}], "group_siblings": [], "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "183847", "eaas_environment_id": null, "production_places": [{"id": 2, "name": "United States", "slug": "united-states"}], "production_dates": [{"date": "2014", "notes": "", "to_year": "2014"}], "labels": [57383], "external_references": []}, {"id": 100609, "acmi_id": "P180325", "title": "Giovanni Pastrone", "title_annotation": "Facsimile", "slug": "giovanni-pastrone", "creator_credit": "", "credit_line": "<p>Courtesy The History Collection / Alamy</p>", "headline_credit": "", "video_links": [], "record_type": "work", "type": "Object", "is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2031-02-16", "is_context_indigenous": false, "material_description": "graphic", "unpublished": false, "external": false, "first_production_date": null, "commissioned": false, "brief_description": "<p>Giovanni Pastrone was a director, producer, actor and inventor who pioneered the use of diffused lighting, panoramas, grandiose sets, miniature models and a moving camera. All of these elements were on show in his epic 1914 film, Cabiria.</p>", "constellations_primary": [{"id": 74, "name": "Tracking cinema", "description": "A constellation looking at the evolution of the tracking / dolly shot."}], "constellations_other": [], "recommendations": [], "title_for_label": "<p>Giovanni Pastrone</p>", "creator_credit_for_label": "", "headline_credit_for_label": "", "description": "<p>Giovanni Pastrone was a director, producer, actor and inventor who pioneered the use of diffused lighting, panoramas, grandiose sets, miniature models and a moving camera. All of these elements were on show in his epic 1914 film, <i>Cabiria</i>. The film broke new artistic ground by using special effects to re-create the eruption of Mount Etna and the naval battle of Syracuse, but its biggest legacy is the ‘Cabiria’ shot: the tracking shot. Pastrone mounted a camera on a dolly to trail the action in a scene, and this new technique was quickly adopted by filmmakers at the time.</p>", "description_for_label": ["<p>Giovanni Pastrone was a director, producer, actor and inventor who pioneered the use of diffused lighting, panoramas, grandiose sets, miniature models and a moving camera. All of these elements were on show in his epic 1914 film, <i>Cabiria</i>. The film broke new artistic ground by using special effects to re-create the eruption of Mount Etna and the naval battle of Syracuse, but its biggest legacy is the ‘Cabiria’ shot: the tracking shot. Pastrone mounted a camera on a dolly to trail the action in a scene, and this new technique was quickly adopted by filmmakers at the time.</p>"], "credit_line_for_label": "<p>Courtesy The History Collection / Alamy</p>", "details": [{"label": "Curatorial section", "display_values": ["The Story of the Moving Image&nbsp;→ Moving Pictures&nbsp;→ MI-04. Materiality&nbsp;→ MI-04-C01"]}, {"label": "Object Types", "display_values": ["2D Object", "Exhibition Prop", "Photographic print/Pictorial"]}, {"label": "Materials", "display_values": ["graphic"]}], "stats": {"tap_count": 832}, "links": [], "creators_primary": [], "creators_other": [], "media_note": null, "holdings": [], "part_of": null, "parts": [], "part_siblings": [], "group": null, "group_works": [], "group_siblings": [], "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "180325", "eaas_environment_id": null, "production_places": [], "production_dates": [], "labels": [40098], "external_references": []}, {"id": 100601, "acmi_id": "P180321", "title": "Georges Méliès in Un homme de têtes (The Four Troublesome Heads)", "title_annotation": "Facsimile", "slug": "georges-melies-in-un-homme-de-tetes-the-four-troublesome-heads", "creator_credit": "", "credit_line": "<p>Courtesy Universal Art Archive / Alamy</p>", "headline_credit": "France, 1898", "video_links": [], "record_type": "work", "type": "Object", "is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2031-02-16", "is_context_indigenous": false, "material_description": "graphic", "unpublished": false, "external": false, "first_production_date": "1898-01-01T09:40:00+09:40", "commissioned": false, "brief_description": "<p>Georges Méliès was truly a magician of the screen. It’s no surprise, considering he enchanted people as an illusionist before devising many special effects and film techniques. At the turn of the 21st century he created seamless screen illusions through in-camera tricks.</p>", "constellations_primary": [{"id": 5, "name": "Clones, doubles and doppelgangers", "description": "Multiple characters and doubles materialise in this Constellation inspired by the pioneering special effects of Georges Méliès."}], "constellations_other": [], "recommendations": [], "title_for_label": "<p>Georges Méliès in Un homme de têtes (The Four Troublesome Heads)</p>", "creator_credit_for_label": "", "headline_credit_for_label": "<p>France, 1898</p>", "description": "<p>Georges Méliès was truly a magician of the screen. It’s no surprise, considering he enchanted people as an illusionist before devising many special effects and film techniques. At the turn of the 21st century he created seamless screen illusions through in-camera tricks like multiple exposures, split-screens, time-lapse photography, dissolves and hand-colouring. He even accidentally invented the jump cut when his camera jammed.</p>\n<p>Between 1896 and 1913, Méliès directed over 500 films. He described them as “little abracadabras” and they were truly magical, inviting audiences on <i>A Trip to the Moon</i> (1902), into <i>The Kingdom of Fairies</i> (1903) and to enjoy a chorus of decapitated heads.</p>", "description_for_label": ["<p>Georges 