t_id": null, "production_places": [{"id": 3, "name": "Australia", "slug": "australia"}], "production_dates": [{"date": "1955", "notes": "", "to_year": "1955"}], "labels": [46994], "external_references": []}, {"id": 107712, "acmi_id": "", "title": "Jedda", "title_annotation": "", "slug": "jedda", "creator_credit": "Charles Chauvel", "credit_line": "<p>Courtesy Chauvel Film Enterprises Pty Ltd</p>\n<p><i>Jedda</i> is held in the collection of the National Film and Sound Archive of Australia</p>", "headline_credit": "Australia, 1955", "video_links": [], "record_type": "group", "type": "Film", "is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2031-02-16", "is_context_indigenous": false, "material_description": "", "unpublished": false, "external": false, "first_production_date": "1955-01-01T10:00:00+10:00", "commissioned": false, "brief_description": "<p>Jedda is beloved by many First Peoples for being Australia&#8217;s first film with Aboriginal lead roles. But despite being a first for First Peoples representation, it also reflects deeply problematic times in Australia, characterised by policies of assimilation and forced removel of Indigenous children.</p>", "constellations_primary": [{"id": 228, "name": "Jedda: Uneven ground, the road to on-screen diversity", "description": "Jedda was the first fully funded Australian feature to be shot on colour film. It was also the first Australian film with Aboriginal actors in lead roles, however, it reflects a deeply problematic time in Australian history."}, {"id": 176, "name": "Jedda", "description": ""}], "constellations_other": [], "recommendations": [], "title_for_label": "<p>Jedda</p>", "creator_credit_for_label": "<p>Charles Chauvel</p>", "headline_credit_for_label": "<p>Australia, 1955</p>", "description": "<p><b>As Australia&#8217;s first feature film with Aboriginal lead roles, <i>Jedda</i> is beloved by many First Peoples. It was one of the first times we saw stories of our people explored and it&#8217;s lauded for its representation of <a href=\"https://www.sbs.com.au/nitv/article/2016/02/17/beck-cole-jedda-lens-50s-white-australia\">&#8220;so many Aboriginal men and women, girls and boys on the screen&#8221;</a>. But despite being a first for First Peoples representation, <i>Jedda</i> also reflects a deeply problematic time in Australian history characterised by formal policies of assimilation and the forced removal of Indigenous children from their families by the government.</b></p>\n<p>The tale of an Aboriginal woman torn between her white upbringing and her heritage, <i>Jedda</i> is the first Australian-made colour feature and first Australian film nominated for the Cannes Film Festival&#8217;s Palme d&#8217;Or. It stars Arrernte actress Rosalie Kunoth-Monks (credited as Ngarla Kunoth) as Jedda and Tiwi actor Robert Tudawali as Marbuck. Due to the film&#8217;s popularity, and a rising interest in Aboriginal &#8216;kitsch&#8217; and culture, Kunoth-Monks and Jedda became entwined. But unlike Tudawali, who had other roles, Kunoth-Monks never acted again and became an advocate for First Peoples in Australia.</p>", "description_for_label": ["<p><b>As Australia&#8217;s first feature film with Aboriginal lead roles, <i>Jedda</i> is beloved by many First Peoples. It was one of the first times we saw stories of our people explored and it&#8217;s lauded for its representation of <a href=\"https://www.sbs.com.au/nitv/article/2016/02/17/beck-cole-jedda-lens-50s-white-australia\">&#8220;so many Aboriginal men and women, girls and boys on the screen&#8221;</a>. But despite being a first for First Peoples representation, <i>Jedda</i> also reflects a deeply problematic time in Australian history characterised by formal policies of assimilation and the forced removal of Indigenous children from their families by the government.</b></p>\n<p>The tale of an Aboriginal woman torn between her white upbringing and her heritage, <i>Jedda</i> is the first Australian-made colour feature and first Australian film nominated for the Cannes Film Festival&#8217;s Palme d&#8217;Or. It stars Arrernte actress Rosalie Kunoth-Monks (credited as Ngarla Kunoth) as Jedda and Tiwi actor Robert Tudawali as Marbuck. Due to the film&#8217;s popularity, and a rising interest in Aboriginal &#8216;kitsch&#8217; and culture, Kunoth-Monks and Jedda became entwined. But unlike Tudawali, who had other roles, Kunoth-Monks never acted again and became an advocate for First Peoples in Australia.</p>"], "credit_line_for_label": "<p>Courtesy Chauvel Film Enterprises Pty Ltd</p>\n<p><i>Jedda</i> is held in the collection of the National Film and Sound Archive of Australia</p>", "details": [{"label": "Curatorial section", "display_values": ["The Story of the Moving Image&nbsp;→ Moving Australia&nbsp;→ MA-10. Ahead of their time&nbsp;→ MA-10-C02"]}], "stats": {"tap_count": 34721}, "links": [{"url": "https://www.justwatch.com/au/movie/jedda-the-uncivilized", "title": "Stream, rent or buy at <span>JustWatch</span>", "source": "justwatch"}], "creators_primary": [{"id": 1860429, "name": "Charles Chauvel", "creator_id": 66792, "creator_wikidata_id": "Q453792", "role": "director", "role_id": 938, "is_primary": true}], "creators_other": [], "media_note": null, "holdings": [], "part_of": null, "parts": [], "part_siblings": [], "group": null, "group_works": [{"id": 99477, "acmi_id": "AEO177032", "title": "‘Aboriginal kitsch’ Jedda plate", "title_annotation": "", "slug": "memorabilia-ceramic-plate-rosalie-kunoth-monks", "creator_credit": "Maker not documented", "credit_line": "", "headline_credit": "Australia, c. 1955", "video_links": [], "record_type": "work", "type": "Object", "is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2031-02-16", "is_context_indigenous": false, "material_description": "ceramic", "unpublished": false, "external": false, "first_production_date": null, "commissioned": false}, {"id": 99714, 