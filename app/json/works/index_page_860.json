annotation": "", "slug": "babakiueria", "creator_credit": "Don Featherstone, Julian Pringle, Australian Broadcasting Corporation and Geoffrey Atherden", "credit_line": "", "headline_credit": "Australia, 1986", "video_links": [], "record_type": "work", "type": "TV show", "is_on_display": false, "last_on_display_place": null, "last_on_display_date": null, "is_context_indigenous": false, "material_description": "", "unpublished": false, "external": false, "first_production_date": "1986-01-01T11:00:00+11:00", "commissioned": false, "public_domain": false, "external_references": [], "brief_description": "<p>&#8216;BabaKiueria&#8217; takes a satirical look at black-white relations in late twentieth century Australia, with a comic depiction of a racial role reversal. In documentary (or &#8216;mockumentary&#8217;) style, the film&#8217;s presenter investigates white lives in the fictitious land of &#8216;BabaKiueria&#8217; (&#8216;Barbeque Area&#8217;), where white people are a minority, living under black laws and suffering from discriminatory practices and attitudes held by the black majority. Written by Geoffrey Atherden (&#8216;Mother and Son&#8217;, &#8216;Grass Roots&#8217;), the film was awarded a United Nations Media Peace Prize in 1987.<br />\nStars Michelle Torres, Bob Maza, Kevin Smith, Cecily Polson, Tony Barry.</p>", "constellations_primary": [], "constellations_other": [], "recommendations": [], "title_for_label": "<p>BabaKiueria</p>", "creator_credit_for_label": "<p>Don Featherstone, Julian Pringle, Australian Broadcasting Corporation and Geoffrey Atherden</p>", "headline_credit_for_label": "<p>Australia, 1986</p>", "description": "<p>&#8216;BabaKiueria&#8217; takes a satirical look at black-white relations in late twentieth century Australia, with a comic depiction of a racial role reversal. In documentary (or &#8216;mockumentary&#8217;) style, the film&#8217;s presenter investigates white lives in the fictitious land of &#8216;BabaKiueria&#8217; (&#8216;Barbeque Area&#8217;), where white people are a minority, living under black laws and suffering from discriminatory practices and attitudes held by the black majority. Written by Geoffrey Atherden (&#8216;Mother and Son&#8217;, &#8216;Grass Roots&#8217;), the film was awarded a United Nations Media Peace Prize in 1987.<br />\nStars Michelle Torres, Bob Maza, Kevin Smith, Cecily Polson, Tony Barry.</p>", "description_for_label": ["<p>&#8216;BabaKiueria&#8217; takes a satirical look at black-white relations in late twentieth century Australia, with a comic depiction of a racial role reversal. In documentary (or &#8216;mockumentary&#8217;) style, the film&#8217;s presenter investigates white lives in the fictitious land of &#8216;BabaKiueria&#8217; (&#8216;Barbeque Area&#8217;), where white people are a minority, living under black laws and suffering from discriminatory practices and attitudes held by the black majority. Written by Geoffrey Atherden (&#8216;Mother and Son&#8217;, &#8216;Grass Roots&#8217;), the film was awarded a United Nations Media Peace Prize in 1987.<br />\nStars Michelle Torres, Bob Maza, Kevin Smith, Cecily Polson, Tony Barry.</p>"], "credit_line_for_label": "", "details": [{"label": "Duration", "display_values": ["00:29:21:00"]}, {"label": "Language", "display_values": ["English"]}, {"label": "Subject categories", "display_values": ["Aboriginal Australia&nbsp;→ Aboriginal Australians - Social conditions", "Aboriginal Australia&nbsp;→ Race discrimination - Australia", "Aboriginal Australia&nbsp;→ Racism - Australia", "Comedy", "Comedy&nbsp;→ Mockumentary", "Comedy&nbsp;→ Satire", "Economics, Philosophy, Politics, Religion & Sociology&nbsp;→ Australia - Race relations", "Television", "Television&nbsp;→ Television programs", "Television&nbsp;→ Television programs&nbsp;→ Television programs - Australia"]}, {"label": "Sound/audio", "display_values": ["Sound"]}, {"label": "Colour", "display_values": ["Colour"]}], "stats": {"tap_count": 0}, "links": [], "