nvironment_id": null, "production_places": [{"id": 3, "name": "Australia", "slug": "australia"}], "production_dates": [], "labels": [59957]}, {"id": 117423, "acmi_id": "Z000062", "title": "The Calling", "title_annotation": "", "slug": "angelica-mesiti-the-calling", "creator_credit": "Angelica Mesiti, Bridget Ikin, Jodie Passmore and Felix Media", "credit_line": "<p>Angelica Mesiti<br />\nThree channel video installation, digital video, sound, 35:36 mins.<br />\nIan Potter Moving Image Commission, ACMI Collection<br />\nCourtesy the artist and Anna Schwartz Gallery</p>", "headline_credit": "Australia, Greece, Spain and Turkey, 2013", "video_links": [], "record_type": "work", "type": "Artwork", "is_on_display": false, "last_on_display_place": "ACMI: Gallery 2", "last_on_display_date": "2014-07-13", "is_context_indigenous": false, "material_description": "Multi-channel installation, three channel video, projection, colour and audio.", "unpublished": false, "external": false, "first_production_date": "2013-01-01T11:00:00+11:00", "commissioned": true, "external_references": [], "brief_description": "<p>&#8216;The Calling&#8217; is a three-channel video installation that focuses on whistling languages, ancient forms of non-verbal communication which are still used in 2013 to communicate in isolated and difficult landscapes.</p>\n<p>To conduct research for &#8216;The Calling&#8217;, artist Angelica Mesiti travelled to the village of Kuskoy in Northern Turkey, the island of La Gomera in The Canary Islands and the Island of Evia in Greece where whistling languages are all still in use.</p>\n<p>For these communities, whistling languages are in a process of transformation from their traditional use as tools for communication across vast lands into tourist attractions and cultural artefacts and are being taught to local school children.</p>\n<p>&#8216;The Calling&#8217; is a poignant exploration of ancient human traditions evolving and adapting to the modern world. The work speaks to the tenacity and creativity of traditional cultures in the face of globalisation, technological progress and environmental flux.</p>\n<p>Angelica Mesiti was the winner of the inaugural Ian Potter Moving Image Commission.</p>", "constellations_primary": [], "constellations_other": [], "recommendations": [], "title_for_label": "<p>The Calling</p>", "creator_credit_for_label": "<p>Angelica Mesiti, Bridget Ikin, Jodie Passmore and Felix Media</p>", "headline_credit_for_label": "<p>Australia, Greece, Spain and Turkey, 2013</p>", "description": "<p><i>The Calling</i> is a three-channel video installation that focuses on whistling languages, ancient forms of non-verbal communication which are still used in 2013 to communicate in isolated and difficult landscapes.</p>\n<p>To conduct research for <i>The Calling</i>, artist Angelica Mesiti travelled to the village of Kuskoy in Northern Turkey, the island of La Gomera in The Canary Islands and the Island of Evia in Greece where whistling languages are all still in use.</p>\n<p>For these communities, whistling languages are in a process of transformation from their traditional use as tools for communication across vast lands into tourist attractions and cultural artefacts and are being taught to local school children.</p>\n<p><i>The Calling</i> is a poignant exploration of ancient human traditions evolving and adapting to the modern world. The work speaks to the tenacity and creativity of traditional cultures in the face of globalisation, technological progress and environmental flux.</p>\n<p>Angelica Mesiti was the winner of the inaugural Ian Potter Moving Image Commission.</p>", "description_for_label": ["<p><i>The Calling</i> is a three-channel video installation that focuses on whistling languages, ancient forms of non-verbal communication which are still used in 2013 to communicate in isolated and difficult landscapes.</p>\n<p>To conduct research for <i>The Calling</i>, artist Angelica Mesiti travelled to the village of Kuskoy in Northern Turkey, the island of La Gomera in The Canary Islands and the Island of Evia in Greece where whistling languages are all still in use.</p>\n<p>For these communities, whistling languages are in a process of transformation from their traditional use as tools for communication across vast lands into tourist attractions and cultural artefacts and are being taught to local school children.</p>\n<p><i>The Calling</i> is a poignant exploration of ancient human traditions evolving and adapting to the modern world. The work speaks to the tenacity and creativity of traditional cultures in the face of globalisation, technological progress and environmental flux.</p>\n<p>Angelica Mesiti was the winner of the inaugural Ian Potter Moving Image Commission.</p>"], "credit_line_for_label": "<p>Angelica Mesiti<br />\nThree channel video installation, digital video, sound, 35:36 mins.<br />\nIan Potter Moving Image Commission, ACMI Collection<br />\nCourtesy the artist and Anna Schwartz Gallery</p>", "details": [{"label": "Duration", "display_values": ["00:35:36:00"]}, {"label": "Subject category", "display_values": ["Digital Art"]}, {"label": "Sound/audio", "display_values": ["Sound"]}, {"label": "Colour", "display_values": ["Colour"]}, {"label": "Object Types", "display_values": ["Installation"]}, {"label": "Materials", "display_values": ["Multi-channel installation, three channel video, projection, colour and audio."]}], "stats": {"tap_count": 2}, "links": [], "creators_primary": [{"id": 2522746, "name": "Angelica Mesiti", "creator_id": 84189, "creator_wikidata_id": "Q121223003", "role": "creator", "role_id": 968, "is_primary": true}, {"id": 2522752, "name": "Angelica Mesiti", "creator_id": 84189, "creator_wikidata_id": "Q121223003", "role": "editor", "role_id": 918, "is_primary": true}, {"id": 2522750, "name": "Bridget Ikin", "creator_id": 51471, "creator_wikidata_id": "Q99337442", "role": "producer", "role_id": 1089, "is_primary": true}, {"id": 2522751, "n