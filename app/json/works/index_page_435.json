on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2031-02-16", "is_context_indigenous": false, "material_description": "", "unpublished": false, "external": false, "first_production_date": null, "commissioned": false}, "group_works": [], "group_siblings": [{"id": 99794, "acmi_id": "FC180292", "title": "Contact sheets", "title_annotation": "Facsimile", "slug": "facsimile-contact-sheets-from-basically-black-tv-show", "creator_credit": "", "credit_line": "<p>Courtesy ABC Library Sales / National Archives of Australia</p>", "headline_credit": "Australia, 1972", "record_type": "work", "type": "Object", "is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2031-02-16", "is_context_indigenous": false, "material_description": "Digital print on paper", "unpublished": false, "external": false, "first_production_date": "1972-01-01T11:00:00+11:00", "commissioned": false}], "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "180417", "eaas_environment_id": null, "production_places": [{"id": 3, "name": "Australia", "slug": "australia"}], "production_dates": [{"date": "1972", "notes": "", "to_year": "1972"}], "labels": [46999], "external_references": []}, {"id": 107708, "acmi_id": "", "title": "Basically Black", "title_annotation": "", "slug": "basically-black", "creator_credit": "ABC", "credit_line": "", "headline_credit": "Australia, 1973", "record_type": "group", "type": "TV show", "is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2031-02-16", "is_context_indigenous": true, "material_description": "", "unpublished": false, "external": false, "first_production_date": "1973-01-01T11:00:00+11:00", "commissioned": false, "brief_description": "<p>Basically Black combined comedy, activism and performing arts while reflecting the political struggles and organising of 1960s and 70s Redfern. The first TV program written and performed by an all-Indigenous cast, this groundbreaking program tackled racism and white paternalism through humour.</p>", "constellations_primary": [{"id": 277, "name": "Laughing in the face of adversity", "description": "Comedy has been used by First Nations storytellers as a tool for social change. On our screens, humour invites audiences to engage with stories and prompts them to grapple with ideas that they otherwise might shy away from."}], "constellations_other": [{"id": 267, "name": "Sequins and sapphires", "description": "Two of the most striking costumes on the big and small screens embody the glamour of leading First Peoples ladies."}, {"id": 293, "name": "Black Power through film and TV", "description": "The Black Power movement of the late 1960s and early 70s was “the most exciting and dynamic era in modern Indigenous history” according to Aboriginal activist Professor Gary Foley, who gained widespread attention for protests during the time and later leveraged film and TV to fight for Indigenous rights."}], "recommendations": [], "title_for_label": "<p>Basically Black</p>", "creator_credit_for_label": "<p>ABC</p>", "headline_credit_for_label": "<p>Australia, 1973</p>", "description": "<p>&#8220;Good morning, I am a human being,&#8221; Bindi Williams tells the audience during an opening skit of <i>Basically Black</i> (1973). Evolving from the newly established National Black Theatre&#8217;s stage production, <i>Basically Black</i> combined comedy, activism and performing arts while reflecting the political struggles and organising of 1960s and 70s Redfern. The first TV program performed by an all-Indigenous cast and led by Blak writers, this groundbreaking program tackled serious issues, like racism and white paternalism, through humour. Despite being well received for its biting satire and commentary, the show didn’t air beyond its premiere. It later emerged that it was considered too political and provocative for the ABC, who had censored parts of the script.</p>", "description_for_label": ["<p>&#8220;Good morning, I am a human being,&#8221; Bindi Williams tells the audience during an opening skit of <i>Basically Black</i> (1973). Evolving from the newly established National Black Theatre&#8217;s stage production, <i>Basically Black</i> combined comedy, activism and performing arts while reflecting the political struggles and organising of 1960s and 70s Redfern. The first TV program performed by an all-Indigenous cast and led by Blak writers, this groundbreaking program tackled serious issues, like racism and white paternalism, through humour. Despite being well received for its biting satire and commentary, the show didn’t air beyond its premiere. It later emerged that it was considered too political and provocative for the ABC, who had censored parts of the script.</p>"], "credit_line_for_label": "", "details": [{"label": "Curatorial section", "display_values": ["The Story of the Moving Image&nbsp;→ Moving Australia&nbsp;→ MA-07. First Peoples: Our stories, our way&nbsp;→ MA-07-02 Basically Black"]}], "stats": {"tap_count": 20140}, "links": [], "creators_primary": [{"id": 1858646, "name": "ABC", "creator_id": 84432, "creator_wikidata_id": null, "role": "creator", "role_id": 968, "is_primary": true}], "creators_other": [], "video_links": [], "media_note": null, "holdings": [], "part_of": null, "parts": [], "part_siblings": [], "group": null, "group_works": [{"id": 113814, "acmi_id": "", "title": "Cast photo and contact sheets", "title_annotation": "", "slug": "cast-photo-and-contact-sheets", "creator_credit": "", "credit_line": "<p>Courtesy ABC Library Sales / National Archives of Australia</p>", "headline_credit": "", "record_type": "group", "type": "Film", "is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2031-02-16", "is_context_indigenous": false, "material_description": "", "unpublished": false, "external": false, "first_production_date": null, "commissioned": false}, {"id": 99725, "acmi_id": 