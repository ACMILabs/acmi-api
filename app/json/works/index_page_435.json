erences": []}, {"id": 117936, "acmi_id": "P181276", "title": "Basketball arcade cabinet", "title_annotation": "", "slug": "basketball-arcade-cabinet", "creator_credit": "Atari", "credit_line": "", "headline_credit": "United States, 1979", "record_type": "work", "type": "Videogame", "is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2031-02-16", "is_context_indigenous": false, "material_description": "", "unpublished": false, "external": false, "first_production_date": "1979-01-01T11:00:00+11:00", "commissioned": false, "brief_description": "<p>Before NBA Jam, there was Atari’s Basketball. Released just in time for the arcade heyday of the early 1980s, it was the first basketball game to use a trackball controller and a 45-degree angled side view.</p>", "constellations_primary": [{"id": 163, "name": "Atari’s Basketball", "description": "Released in 1979, just in time for the arcade heydays of the early 1980s, Atari’s Basketball was the first basketball game to use a trackball as the input mechanic and a 45-degree angled side view. This set the standard for basketball games that followed."}], "constellations_other": [], "recommendations": [], "title_for_label": "<p>Basketball</p>", "creator_credit_for_label": "<p>Atari</p>", "headline_credit_for_label": "<p>United States, 1979</p>", "description": "<p>Before <i>NBA Jam</i> and the <i>NBA 2K</i> basketball series, there was Atari’s Basketball. Released in 1979, just in time for the arcade heydays of the early 1980s, it was the first basketball game to use a trackball as the input mechanic and a 45-degree angled side view. Not only did this become part of the playbook for basketball games that followed, but Basketball was also the first sports game where the pixelated sprites players controlled actually looked like people. The game unit’s neon graphic design is also uniquely of its time and reflects the burgeoning videogame aesthetic.</p>", "description_for_label": ["<p>Before <i>NBA Jam</i> and the <i>NBA 2K</i> basketball series, there was Atari’s Basketball. Released in 1979, just in time for the arcade heydays of the early 1980s, it was the first basketball game to use a trackball as the input mechanic and a 45-degree angled side view. Not only did this become part of the playbook for basketball games that followed, but Basketball was also the first sports game where the pixelated sprites players controlled actually looked like people. The game unit’s neon graphic design is also uniquely of its time and reflects the burgeoning videogame aesthetic.</p>"], "credit_line_for_label": "", "details": [{"label": "Curatorial section", "display_values": ["The Story of the Moving Image&nbsp;→ Games Lab&nbsp;→ GL-02. Cluster 2&nbsp;→ GL-02-C02"]}, {"label": "Object Types", "display_values": ["3D Object", "Arcade Game/Game"]}], "stats": {"tap_count": 74}, "links": [], "creators_primary": [{"id": 1856118, "name": "Atari", "creator_id": 84343, "creator_wikidata_id": null, "role": "creator", "role_id": 968, "is_primary": true}], "creators_other": [], "video_links": [], "media_note": null, "holdings": [], "part_of": null, "parts": [], "part_siblings": [], "group": {"id": 117937, "acmi_id": "", "title": "Retro gaming", "title_annotation": "", "slug": "retro-gaming", "creator_credit": "", "credit_line": "", "headline_credit": "", "record_type": "group", "type": "Object", "is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2031-02-16", "is_context_indigenous": false, "material_description": "", "unpublished": false, "external": false, "first_production_date": null, "commissioned": false}, "group_works": [], "group_siblings": [], "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "181276", "eaas_environment_id": null, "production_places": [{"id": 2, "name": "United States", "slug": "united-states"}], "production_dates": [{"date": "1979", "notes": "", "to_year": "1979"}], "labels": [57431], "external_