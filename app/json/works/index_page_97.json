play_place": null, "last_on_display_date": null, "is_context_indigenous": true, "material_description": "", "unpublished": false, "external": false, "first_production_date": "1998-01-01T11:00:00+11:00", "commissioned": false, "external_references": [], "brief_description": "<p>Returning from her adopted home in London, to attend the funeral of her sister, an Aboriginal woman finds herself isolated from both kin and culture. This emotionally mature and complex work moves beyond didactism and easily digestable polemics to suggest that cultural dislocation is written as much in the body, in language and the psyche as it is in economics and politics. By suggesting that the effects for the &#8220;stolen generation&#8221; were as destructive for inter-Aboriginal relationships as they were for black and white relations in this country, &#8220;Grace&#8221; is a devastating and confronting exploration of what it means to have been &#8220;stolen&#8221;.</p>", "constellations_primary": [], "constellations_other": [], "recommendations": [], "title_for_label": "<p>Grace</p>", "creator_credit_for_label": "<p>Wesley Enoch</p>", "headline_credit_for_label": "<p>Australia, 1998</p>", "description": "<p>Returning from her adopted home in London, to attend the funeral of her sister, an Aboriginal woman finds herself isolated from both kin and culture. This emotionally mature and complex work moves beyond didactism and easily digestable polemics to suggest that cultural dislocation is written as much in the body, in language and the psyche as it is in economics and politics. By suggesting that the effects for the &#8220;stolen generation&#8221; were as destructive for inter-Aboriginal relationships as they were for black and white relations in this country, &#8220;Grace&#8221; is a devastating and confronting exploration of what it means to have been &#8220;stolen&#8221;.</p>", "description_for_label": ["<p>Returning from her adopted home in London, to attend the funeral of her sister, an Aboriginal woman finds herself isolated from both kin and culture. This emotionally mature and complex work moves beyond didactism and easily digestable polemics to suggest that cultural dislocation is written as much in the body, in language and the psyche as it is in economics and politics. By suggesting that the effects for the &#8220;stolen generation&#8221; were as destructive for inter-Aboriginal relationships as they were for black and white relations in this country, &#8220;Grace&#8221; is a devastating and confronting exploration of what it means to have been &#8220;stolen&#8221;.</p>"], "credit_line_for_label": "", "details": [{"label": "Duration", "display_values": ["00:16:00:00"]}, {"label": "Language", "display_values": ["English"]}], "stats": {"tap_count": 0}, "links": [], "creators_primary": [{"id": 2520655, "name": "Wesley Enoch", "creator_id": 7078, "creator_wikidata_id": null, "role": "creator", "role_id": 968, "is_primary": true}], "creators_other": [], "media_note": "Sorry, we don't have images or video for this item.", "holdings": [{"name": "Digital Betacam [PAL]; Master", "identifier": "", "description": ""}, {"name": "VHS [PAL]; Reference - timecoded", "identifier": "", "description": ""}, {"name": "Digital Betacam [PAL]; Sub-master", "identifier": "", "description": ""}], "part_of": null, "parts": [], "part_siblings": [], "group": null, "group_works": [], "group_siblings": [], "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "452", "eaas_environment_id": null, "production_places": [{"id": 3, "name": "Australia", "slug": "australia"}], "production_dates": [{"date": "1998", "notes": "", "to_year": "1998"}], "labels": [47717]}, {"id": 117918, "acmi_id": "B2004334", "title": "Spotlights: Gillian Armstrong", "title_annotation": "", "slug": "spotlights-gillian-armstrong", "creator_credit": "", "credit_line": "", "headline_credit": "Australia, 2020", "video_links": [], "record_type": "work", "type": "Film", "is_on_display": t