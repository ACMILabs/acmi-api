cord_type": "group", "type": "Film", "is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2025-02-13", "is_context_indigenous": false, "material_description": "", "unpublished": false, "external": false, "first_production_date": null, "commissioned": false}, "group_works": [], "group_siblings": [], "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "186611", "eaas_environment_id": null, "production_places": [{"id": 18, "name": "Germany", "slug": "germany"}], "production_dates": [{"date": "c. 1920", "notes": "", "to_year": "c. 1920"}], "labels": [59063], "external_references": []}, {"id": 113726, "acmi_id": "", "title": "Inspired design and costume sketches", "title_annotation": "", "slug": "inspired-design-and-costume-sketches", "creator_credit": "", "credit_line": "", "headline_credit": "", "video_links": [], "record_type": "group", "type": "Film", "is_on_display": false, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2022-02-10", "is_context_indigenous": false, "material_description": "", "unpublished": false, "external": false, "first_production_date": null, "commissioned": false, "brief_description": "<p>Costume designers don&#8217;t always rely on sketches of their creations. True History of the Kelly Gang designer Alice Babidge sketches to distil references, while Judy &amp; Punch designer Edie Kurzer gets more inspiration from objects.</p>", "constellations_primary": [{"id": 286, "name": "Inspired design and costume sketches", "description": "Costume designers Edie Kurzer and Alice Babidge have worked on some of the most acclaimed Australian and international films and TV, joining the ranks of fellow Australians creatives."}], "constellations_other": [], "recommendations": [], "title_for_label": "<p>Inspired design and costume sketches</p>", "creator_credit_for_label": "", "headline_credit_for_label": "", "description": "<p><i>Judy &amp; Punch</i> (2019) costume designer Edie Kurzer often explores mood and atmosphere with images instead of using them as direct influences. While she may use sketches to work through details, she gains more inspiration from objects like feathers, buttons or fabrics, and then works with a fabric cutter, in this case Justine Coultham, who sketches how the costume should be cut and assembled.   </p>\n<p>Rather than draw what the finished costume should look like, <i>True History of the Kelly Gang</i> (2019) costume designer Alice Babidge uses sketches to distil various reference images into one thought. For Babidge, drawing helps &#8220;refine an idea&#8221;, while the torn faces on the photos help the actors imagine how they&#8217;ll inhabit the character.</p>", "description_for_label": ["<p><i>Judy &amp; Punch</i> (2019) costume designer Edie Kurzer often explores mood and atmosphere with images instead of using them as direct influences. While she may use sketches to work through details, she gains more inspiration from objects like feathers, buttons or fabrics, and then works with a fabric cutter, in this case Justine Coultham, who sketches how the costume should be cut and assembled.   </p>\n<p>Rather than draw what the finished costume should look like, <i>True History of the Kelly Gang</i> (2019) costume designer Alice Babidge uses sketches to distil various reference images into one thought. For Babidge, drawing helps &#8220;refine an idea&#8221;, while the torn faces on the photos help the actors imagine how they&#8217;ll inhabit the character.</p>"], "credit_line_for_label": "", "details": [{"label": "Curatorial section", "display_values": ["The Story of the Moving Image&nbsp;→ Moving Worlds&nbsp;→ MW-04. Costume Design&nbsp;→ MW-04-C06"]}], "stats": {"tap_count": 15562}, "links": [], "creators_primary": [], "creators_other": [], "media_note": null, "holdings": [], "part_of": null, "parts": [], "part_siblings": [], "group": null, "group_works": [{"id": 113725, "acmi_id": "", "title": "Judy & Punch costume sketches", "title_annotation": "", "slug": "judy-punch-costume-sketches", "creator_credit": "Justine Coultham", "credit_line": "<p>Courtesy Vice Media LLC, Blue-Tongue Films and Pariah Productions</p>", "headline_credit": "Australia, 2018", "video_links": [], "record_type": "group", "type": "Object", "is_on_display": false, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2022-02-10", "is_context_indigenous": false, "material_description": "", "unpublished": false, "external": false, "first_production_date": "2018-01-01T11:00:00+11:00", "commissioned": false}, {"id": 113727, "acmi_id": "", "title": "Judy & Punch reference images from Edie Kurzer's studio", "title_annotation": "", "slug": "judy-punch-reference-images-from-edie-kurzers-studio", "creator_credit": "", "credit_line": "", "headline_credit": "", "video_links": [], "record_type": "group", "type": "Film", "is_on_display": false, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2022-02-10", "is_context_indigenous": false, "material_description": "", "unpublished": false, "external": false, "first_production_date": null, "commissioned": false}, {"id": 100865, "acmi_id": "", "title": "True History of the Kelly Gang costume sketches", "title_annotation": "", "slug": "true-history-of-the-kelly-gang-costume-sketches", "creator_credit": "Alice Babidge", "credit_line": "<p>Courtesy Jetty Distribution Pty Limited</p>", "headline_credit": "Australia, 2018", "video_links": [], "record_type": "group", "type": "Object", "is_on_display": false, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2022-02-10", "is_context_indigenous": false, "material_description": "", "unpublished": false, "external": false, "first_production_date": "2018-01-01T11:00:00+11:00", "commissioned": false}], "group_siblings": [], "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "183591", "eaas_environment_id": null, "production_places": [], "production_dates": [], "lab