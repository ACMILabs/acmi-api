on": "", "slug": "gone-home", "creator_credit": "Fullbright", "credit_line": "<p>Fullbright, United States, 2013</p>", "headline_credit": "United States, 2014", "record_type": "group", "type": "Videogame", "is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2031-02-16", "is_context_indigenous": false, "material_description": "", "unpublished": false, "first_production_date": "2014-01-01T11:00:00+11:00", "brief_description": "<p>When Katie returns home from overseas, she finds her house deserted and her family gone. That’s just one horror trope that <I>Gone Home</i> uses to mislead players as Katie searches the house for her family in this walking simulator.</p>", "constellations_primary": [{"id": 112, "name": "Teens, zines and 90s scenes", "description": "Exploring representations of teen life and culture."}], "constellations_other": [], "recommendations": [], "title_for_label": "<p>Gone Home</p>", "creator_credit_for_label": "<p>Fullbright</p>", "headline_credit_for_label": "<p>United States, 2014</p>", "description": "<p>When Katie returns home from overseas, she finds her house deserted and her family gone.</p>\n<p>That’s just one horror trope that <i>Gone Home</i> uses to mislead players as Katie searches the house for her family. Unlike typical games, <i>Gone Home</i> features minimal activity and was an early ‘walking simulator’, with the story unfolding at the player’s own pace as they examine notes and objects, rather than relying on traditional exposition.</p>\n<p>Developed by indie studio Fullbright, <i>Gone Home</i> was highly praised by critics and fans for its unique style and positive representation of LGBTQIA+ themes.</p>", "description_for_label": ["<p>When Katie returns home from overseas, she finds her house deserted and her family gone.</p>\n<p>That’s just one horror trope that <i>Gone Home</i> uses to mislead players as Katie searches the house for her family. Unlike typical games, <i>Gone Home</i> features minimal activity and was an early ‘walking simulator’, with the story unfolding at the player’s own pace as they examine notes and objects, rather than relying on traditional exposition.</p>\n<p>Developed by indie studio Fullbright, <i>Gone Home</i> was highly praised by critics and fans for its unique style and positive representation of LGBTQIA+ themes.</p>"], "credit_line_for_label": "<p>Fullbright, United States, 2013</p>", "details": [{"label": "Curatorial section", "display_values": ["The Story of the Moving Image&nbsp;→ Games Lab&nbsp;→ 01. Cluster 1&nbsp;→ GL-01-C02"]}], "stats": {"tap_count": 7755}, "links": [{"url": "https://store.steampowered.com/app/232430/Gone_Home/", "title": "store.steampowered.com", "source": "vernon"}], "creators_primary": [{"id": 1033597, "name": "Fullbright", "creator_id": 84313, "role": "creator", "role_id": 968, "is_primary": true}], "creators_other": [], "video_links": [], "media_note": null, "holdings": [], "part_of": null, "parts": [], "part_siblings": [], "group": null, "group_works": [{"id": 117890, "acmi_id": "", "title": "Game screenshots", "title_annotation": "", "slug": "game-screenshots", "creator_credit": "", "credit_line": "", "headline_credit": "", "record_type": "group", "type": "Film", "is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2031-02-16", "is_context_indigenous": false, "material_description": "", "unpublished": false, "first_production_date": null}, {"id": 117888, "acmi_id": "", "title": "Gone Home deluxe edition and designer's notebook", "title_annotation": "", "slug": "gone-home-deluxe-edition-and-designers-notebook", "creator_credit": "", "credit_line": "", "headline_credit": "", "record_type": "group", "type": "Film", "is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2031-02-16", "is_context_indigenous": false, "material_description": "", "unpublished": false, "first_production_date": null}, {"id": 118258, "acmi_id":