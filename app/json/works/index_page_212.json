": [{"id": 8, "name": "Japan", "slug": "japan"}], "production_dates": [], "labels": [39522]}, {"id": 108287, "acmi_id": "E000060", "title": "Sony Betacam BVP-3P", "title_annotation": "", "slug": "betacam-bvp-3p-camera", "creator_credit": "Sony", "credit_line": "<p>ACMI Collection</p>", "headline_credit": "Japan, c. 1983", "record_type": "work", "type": "Object", "is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2031-02-16", "is_context_indigenous": false, "material_description": "", "unpublished": false, "first_production_date": null, "brief_description": "<p>Sony introduced the professional use Betacam cassette format in 1982 for the broadcast video industry. Although related to the Betamax format, its key differences were consumer orientated as it competed against VHS (Video Home System) in the videotape format wars of the period.</p>", "constellations_primary": [{"id": 196, "name": "Oh, no! Beta!", "description": "The Sony Betacam BVP-3P was a broadcast-quality camera that used Sony's Beta system, most commonly found in consumer cameras. During the Format Wars in the 1970s and 80s, Beta lost to the cheaper and more accessible Video Home System (VHS)."}], "constellations_other": [], "recommendations": [], "title_for_label": "<p>Sony Betacam BVP-3P</p>", "creator_credit_for_label": "<p>Sony</p>", "headline_credit_for_label": "<p>Japan, c. 1983</p>", "description": "<p>Electronic News Gathering – also known as ENG – is a term that emerged in the 1970s to describe news reporting that used electronic field production technology. ENG could refer to anything from a single camera operator to a production van transmitting from the field, as television departments moved to more immediate broadcast of news without having to wait for film to be developed. Sony introduced the professional use Betacam cassette format in 1982 for the broadcast video industry. Although related to the Betamax format, its key differences were consumer orientated as it competed against VHS (Video Home System) in the videotape format wars of the period. Interestingly Betacam continued to be used in a professional setting for much longer than Betamax lasted in the consumer market.</p>", "description_for_label": ["<p>Electronic News Gathering – also known as ENG – is a term that emerged in the 1970s to describe news reporting that used electronic field production technology. ENG could refer to anything from a single camera operator to a production van transmitting from the field, as television departments moved to more immediate broadcast of news without having to wait for film to be developed. Sony introduced the professional use Betacam cassette format in 1982 for the broadcast video industry. Although related to the Betamax format, its key differences were consumer orientated as it competed against VHS (Video Home System) in the videotape format wars of the period. Interestingly Betacam continued to be used in a professional setting for much longer than Betamax lasted in the consumer market.</p>"], "credit_line_for_label": "<p>ACMI Collection</p>", "details": [{"label": "Curatorial section", "display_values": ["The Story of the Moving Image&nbsp;→ Moving Minds&nbsp;→ MM-05. Camera Tech&nbsp;→ MM-05-C01"]}, {"label": "Measurements", "display_values": [" 500 x 700 x 180mm"]}], "stats": {"tap_count": 2576}, "links": [], "creators_primary": [{"id": 1436339, "name": "Sony", "creator_id": 71968, "creator_wikidata_id": null, "role": "manufacturer", "role_id": 627, "is_primary": true}], "creators_other": [], "video_links": [], "media_note": null, "holdings": [], "part_of": null, "parts": [], "part_siblings": [], "group": null, "group_works": [], "group_siblings": [], "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "13262", "eaas_environment_id": null, "production_places": [{"id": 8, "name": "Japan", "slug": "japan"}], "production_dates": [{"date": "c. 1983", "notes": "", "to_year": "c. 1983"}], "labels": 