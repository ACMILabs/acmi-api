aces": [], "production_dates": [{"date": "1996", "notes": "", "to_year": "1996"}], "labels": [39740]}, {"id": 74593, "acmi_id": "011790", "title": "West wind: the story of Tom Thomson", "title_annotation": "", "slug": "west-wind-the-story-of-tom-thomson", "creator_credit": "Graham McInnes and National Film Board of Canada", "credit_line": "", "headline_credit": "Canada, 1943", "video_links": [], "record_type": "work", "type": "Film", "is_on_display": false, "last_on_display_place": null, "last_on_display_date": null, "is_context_indigenous": false, "material_description": "", "unpublished": false, "external": false, "first_production_date": "1943-01-01T11:00:00+11:00", "commissioned": false, "external_references": [], "brief_description": "<p>Tom Thomson&#8217;s jack pine, painted against a background of lake and sky, has come to symbolize the Canadian North. This film outlines the story of Tom Thomson&#8217;s brief life (1877-1917) and career, which began in Toronto, where he worked as a commercial artist. Weekend sketching trips in the country turned into longer journeys farther north, and Thomson finally moved to Algonquin Park, in northern Ontario. When he wasn&#8217;t painting and canoeing, he was a park guide. We watch wonderful photography of his favourite landscapes, interspersed with images of the paintings they inspired. Thomson spent less than four years as an artist and was barely 40 when a canoe accident ended his life. Fellow artists Lawren Harris, A.Y. Jackson and Arthur Lismer pay tribute to this genius, who, in Jackson&#8217;s words, &#8220;contributed more to Canadian painting than any other artist.&#8221;</p>", "constellations_primary": [], "constellations_other": [], "recommendations": [], "title_for_label": "<p>West wind: the story of Tom Thomson</p>", "creator_credit_for_label": "<p>Graham McInnes and National Film Board of Canada</p>", "headline_credit_for_label": "<p>Canada, 1943</p>", "description": "<p>Tom Thomson&#8217;s jack pine, painted against a background of lake and sky, has come to symbolize the Canadian North. This film outlines the story of Tom Thomson&#8217;s brief life (1877-1917) and career, which began in Toronto, where he worked as a commercial artist. Weekend sketching trips in the country turned into longer journeys farther north, and Thomson finally moved to Algonquin Park, in northern Ontario. When he wasn&#8217;t painting and canoeing, he was a park guide. We watch wonderful photography of his favourite landscapes, interspersed with images of the paintings they inspired. Thomson spent less than four years as an artist and was barely 40 when a canoe accident ended his life. Fellow artists Lawren Harris, A.Y. Jackson and Arthur Lismer pay tribute to this genius, who, in Jackson&#8217;s words, &#8220;contributed more to Canadian painting than any other artist.&#8221;</p>", "description_for_label": ["<p>Tom Thomson&#8217;s jack pine, painted against a background of lake and sky, has come to symbolize the Canadian North. This film outlines the story of Tom Thomson&#8217;s brief life (1877-1917) and career, which began in Toronto, where he worked as a commercial artist. Weekend sketching trips in the country turned into longer journeys farther north, and Thomson finally moved to Algonquin Park, in northern Ontario. When he wasn&#8217;t painting and canoeing, he was a park guide. We watch wonderful photography of his favourite landscapes, interspersed with images of the paintings they inspired. Thomson spent less than four years as an artist and was barely 40 when a canoe accident ended his life. Fellow artists Lawren Harris, A.Y. Jackson and Arthur Lismer pay tribute to this genius, who, in Jackson&#8217;s words, &#8220;contributed more to Canadian painting than any other artist.&#8221;</p>"], "credit_line_for_label": "", "details": [{"label": "Duration", "display_values": ["00:19:58:00"]}, {"label": "Language", "display_values": ["English"]}, {"label": "Subject categories", "display_val