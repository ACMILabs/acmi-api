nks": [], "creators_primary": [{"id": 1851909, "name": "Nintendo", "creator_id": 69760, "creator_wikidata_id": null, "role": "manufacturer", "role_id": 627, "is_primary": true}], "creators_other": [], "media_note": null, "holdings": [], "part_of": null, "parts": [], "part_siblings": [], "group": {"id": 119780, "acmi_id": "", "title": "Aussie Rules Footy", "title_annotation": "", "slug": "aussie-rules-footy", "creator_credit": "Beam Software", "credit_line": "", "headline_credit": "Australia, 1991", "video_links": [], "record_type": "group", "type": "Videogame", "is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2023-02-09", "is_context_indigenous": false, "material_description": "", "unpublished": false, "external": false, "first_production_date": "1991-01-01T11:00:00+11:00", "commissioned": false}, "group_works": [], "group_siblings": [], "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "187392", "eaas_environment_id": null, "production_places": [{"id": 8, "name": "Japan", "slug": "japan"}], "production_dates": [{"date": "1985", "notes": "", "to_year": "1985"}], "labels": [59405], "external_references": []}, {"id": 100238, "acmi_id": "P180331", "title": "Natalie Kalmus", "title_annotation": "Facsimile", "slug": "graphic-natalie-kalmus-technicolor-pioneer", "creator_credit": "", "credit_line": "<p>Courtesy RGR Collection / Alamy</p>", "headline_credit": "", "video_links": [], "record_type": "work", "type": "Object", "is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2031-02-16", "is_context_indigenous": false, "material_description": "Digital print", "unpublished": false, "external": false, "first_production_date": null, "commissioned": false, "brief_description": "<p>When studios hired Technicolor cameras and equipment, they also hired Natalie Kalmus. During the glory days of Technicolor’s three-strip process, between 1934 and 1955, she was the colour supervisor on over 400 films, including The Wizard of Oz (1939).</p>", "constellations_primary": [{"id": 82, "name": "Communicating colour", "description": "An exploration of colour on-screen inspired by Technicolor supervisor, Natalie Kalmus."}], "constellations_other": [], "recommendations": [], "title_for_label": "<p>Natalie Kalmus</p>", "creator_credit_for_label": "", "headline_credit_for_label": "", "description": "<p>When studios hired Technicolor cameras and equipment, they also hired Natalie Kalmus. During the glory days of Technicolor’s three-strip process, between 1934 and 1955, she was the colour supervisor on over 400 films, including <I>The Wizard of Oz</i> (1939) and <I>Gone with the Wind</i> (1939).</p>\n<p>After seeing filmmakers use the earlier two-strip Technicolor process to produce “unnatural and disastrous results”, she urged filmmakers to employ “colour restraint”. Kalmus believed that colour theories used in paintings could also be used in film and encouraged filmmakers to carefully consider which colours would convey a story’s emotions and themes.</p>", "description_for_label": ["<p>When studios hired Technicolor cameras and equipment, they also hired Natalie Kalmus. During the glory days of Technicolor’s three-strip process, between 1934 and 1955, she was the colour supervisor on over 400 films, including <I>The Wizard of Oz</i> (1939) and <I>Gone with the Wind</i> (1939).</p>\n<p>After seeing filmmakers use the earlier two-strip Technicolor process to produce “unnatural and disastrous results”, she urged filmmakers to employ “colour restraint”. Kalmus believed that colour theories used in paintings could also be used in film and encouraged filmmakers to carefully consider which colours would convey a story’s emotions and themes.</p>"], "credit_line_for_label": "<p>Courtesy RGR Collection / Alamy</p>", "details": [{"label": "Curatorial section", "display_values": ["The Story of the Moving Image&nbsp;→ Moving Pictures&nbsp;→ MI-05. Sound and Colour&nbsp;→ MI-05-C03"]}, {"label": "Measurements", "display_values": ["237 x 190mm"]}, {"label": "Object Types", "display_values": ["2D Object", "Exhibition Prop", "Photographic print/Pictorial"]}, {"label": "Materials", "display_values": ["Digital print"]}], "stats": {"tap_count": 42110}, "links": [], "creators_primary": [], "creators_other": [{"id": 1724692, "name": "Maker not documented", "creator_id": 71446, "creator_wikidata_id": null, "role": "", "role_id": 604, "is_primary": false}], "media_note": null, "holdings": [], "part_of": null, "parts": [], "part_siblings": [], "group": null, "group_works": [], "group_siblings": [], "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "180331", "eaas_environment_id": null, "production_places": [], "production_dates": [], "labels": [39734], "external_references": []}]}