production_places": [], "production_dates": [], "labels": [58333], "external_references": []}, {"id": 108284, "acmi_id": "E000092", "title": "KOK 28mm hand cranked projector", "title_annotation": "", "slug": "kok-28mm-hand-cranked-projector", "creator_credit": "Pathé Frères", "credit_line": "<p>ACMI Collection</p>", "headline_credit": "France, c. 1912", "video_links": [], "record_type": "work", "type": "Object", "is_on_display": true, "last_on_display_place": "ACMI Ground Floor", "last_on_display_date": "2019-04-22", "is_context_indigenous": false, "material_description": "", "unpublished": false, "external": false, "first_production_date": null, "commissioned": false, "brief_description": "<p>In 1912, the popular Pathé Kok camera and projector arrived, which used the more standard 28mm film and cast a 30-inch picture.</p>", "constellations_primary": [], "constellations_other": [], "recommendations": [], "title_for_label": "<p>KOK 28mm hand cranked projector</p>", "creator_credit_for_label": "<p>Pathé Frères</p>", "headline_credit_for_label": "<p>France, c. 1912</p>", "description": "<p>28mm hand crank projector designed by Pathe. Designed for home use, the Pathe KOK projector uses non-flammable cellulose acetate safety film and an electric light. The projector is powered by a crank handle. Contains: projector, hand crank, metal cover, 2 x metal reels.<br />\nSticker on reel says &#8220;The property of Visual Education Centre, Education Dept. Victoria&#8221;<br />\nSee video of same model  (modified) of projector in use: https://www.youtube.com/watch?v=SG9Nvly1UB4<br />\nWas ACMI Lending Collection/ACMI Collection in 2014 audit - NR says unclear, but should keep.<br />\n28 mm gauge metal projector on wooden base.Object functions.</p>", "description_for_label": ["<p>28mm hand crank projector designed by Pathe. Designed for home use, the Pathe KOK projector uses non-flammable cellulose acetate safety film and an electric light. The projector is powered by a crank handle. Contains: projector, hand crank, metal cover, 2 x metal reels.<br />\nSticker on reel says &#8220;The property of Visual Education Centre, Education Dept. Victoria&#8221;<br />\nSee video of same model  (modified) of projector in use: https://www.youtube.com/watch?v=SG9Nvly1UB4<br />\nWas ACMI Lending Collection/ACMI Collection in 2014 audit - NR says unclear, but should keep.<br />\n28 mm gauge metal projector on wooden base.Object functions.</p>"], "credit_line_for_label": "<p>ACMI Collection</p>", "details": [{"label": "Curatorial section", "display_values": ["The Story of the Moving Image&nbsp;→ Moving Pictures&nbsp;→ MI-04. Materiality&nbsp;→ MI-04-C01"]}, {"label": "Measurements", "display_values": ["410 x 260 x 560mm"]}, {"label": "Object Types", "display_values": ["3D Object", "Projection equipment/Film and television equipment"]}], "stats": {"tap_count": 0}, "links": [], "creators_primary": [{"id": 1935420, "name": "Pathé Frères", "creator_id": 1976, "creator_wikidata_id": null, "role": "manufacturer", "role_id": 627, "is_primary": true}], "creators_other": [], "media_note": null, "holdings": [], "part_of": null, "parts": [], "part_siblings": [], "group": {"id": 100494, "acmi_id": "", "title": "Early film equipment", "title_annotation": "", "slug": "early-film-equipment", "creator_credit": "", "credit_line": "", "headline_credit": "", "video_links": [], "record_type": "group", "type": "Object", "is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2031-02-16", "is_context_indigenous": false, "material_description": "", "unpublished": false, "external": true, "first_production_date": null, "commissioned": false}, "group_works": [], "group_siblings": [], "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "15490", "eaas_environment_id": null, "production_places": [{"id": 17, "name": "France", "slug": "france"}], "production_dates": [{"date": "c. 1912", "notes": "", "to_year": "c. 1912"}], "labels": [4