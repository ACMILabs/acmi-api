 [{"id": 179, "name": "Unknown", "slug": "unknown"}], "production_dates": [{"date": "1970", "notes": "Estimated date", "to_year": "1970"}], "labels": [15259], "external_references": []}, {"id": 121825, "acmi_id": "", "title": "Happy Transgender Awareness Week", "title_annotation": "", "slug": "happy-transgender-awareness-week", "creator_credit": "Alexpepsii", "credit_line": "", "headline_credit": "14 NOV 2022", "video_links": [], "record_type": "work", "type": "Film", "is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": null, "is_context_indigenous": false, "material_description": "Digital screen recording of @alexpepsii's Tiktok post", "unpublished": false, "external": false, "first_production_date": null, "commissioned": false, "brief_description": "<p>Transgender Awareness Week aims to raise the visibility of transgender people and address issues faced by the community. Social media plays an important role, with users sharing their own stories and amplifying the voices of transgender people.  This video celebrates Transgender Awareness Week through a compilation of transgender characters in recent film and television, from Raúl in Wendell &amp; Wild to Natalie in Big Mouth.<br />\nTransgender Awareness Week is a one-week celebration held annually from 13 - 19 November. It leads up to Transgender Day of Remembrance on 20 November which memorialises the victims of transphobic violence.</p>", "constellations_primary": [], "constellations_other": [], "recommendations": [], "title_for_label": "<p>Happy Transgender Awareness Week</p>", "creator_credit_for_label": "<p>Alexpepsii</p>", "headline_credit_for_label": "<p>14 NOV 2022</p>", "description": "<p><b>This video features in a responsive display in the museum that presents the moments, stories and memes capturing the zeitgeist by going viral on the internet.</b></p>\n<p>Transgender Awareness Week raises the visibility of transgender people and address issues faced by the community. Social media plays a vital role, with users sharing their stories and amplifying the voices of transgender people. This video celebrates Transgender Awareness Week through a compilation of transgender characters in recent film and television, from Raúl in <i>Wendell &amp; Wild</i> to Natalie in <i>Big Mouth</i>. Transgender Awareness Week is held from 13 to 19 November, and leads to Transgender Day of Remembrance, which memorialises victims of transphobic violence.</p>", "description_for_label": ["<p>Transgender Awareness Week raises the visibility of transgender people and address issues faced by the community. Social media plays a vital role, with users sharing their stories and amplifying the voices of transgender people. This video celebrates Transgender Awareness Week through a compilation of transgender characters in recent film and television, from Raúl in <i>Wendell &amp; Wild</i> to Natalie in <i>Big Mouth</i>. Transgender Awareness Week is held from 13 to 19 November, and leads to Transgender Day of Remembrance, which memorialises victims of transphobic violence.</p>"], "credit_line_for_label": "", "details": [{"label": "Curatorial section", "display_values": ["The Story of the Moving Image&nbsp;→ Moving Minds&nbsp;→ MM-09. Catch of the Day"]}, {"label": "Object Types", "display_values": ["Moving image file/Digital"]}, {"label": "Materials", "display_values": ["Digital screen recording of @alexpepsii's Tiktok post"]}], "stats": {"tap_count": 717}, "links": [], "creators_primary": [{"id": 1908071, "name": "Alexpepsii", "creator_id": 86988, "creator_wikidata_id": null, "role": "creator", "role_id": 968, "is_primary": true}], "creators_other": [], "media_note": null, "holdings": [], "part_of": null, "parts": [], "part_siblings": [], "group": null, "group_works": [], "group_siblings": [], "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "192799", "eaas_environment_id": null, "production_places": [], "production_dates": [{"date": "14 NOV