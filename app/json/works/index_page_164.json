ors_primary": [{"id": 2500173, "name": "Weta Workshop", "creator_id": 72937, "creator_wikidata_id": null, "role": "artist", "role_id": 505, "is_primary": true}], "creators_other": [], "media_note": null, "holdings": [], "part_of": null, "parts": [], "part_siblings": [], "group": {"id": 112116, "acmi_id": "", "title": "Character concept art with feedback from Jacob Nash", "title_annotation": "", "slug": "character-concept-art-with-feedback-from-jacob-nash", "creator_credit": "Weta Workshop", "credit_line": "<p>Courtesy Goalpost Pictures</p>", "headline_credit": "", "video_links": [], "record_type": "group", "type": "Film", "is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2024-11-10", "is_context_indigenous": false, "material_description": "", "unpublished": false, "external": true, "first_production_date": null, "commissioned": false, "external_references": []}, "group_works": [], "group_siblings": [], "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "180551", "eaas_environment_id": null, "production_places": [{"id": 3, "name": "Australia", "slug": "australia"}], "production_dates": [{"date": "2016", "notes": "", "to_year": "2016"}], "labels": [40236]}, {"id": 121797, "acmi_id": "Z000138", "title": "Epiphytes", "title_annotation": "", "slug": "epiphytes", "creator_credit": "Tully Arnot", "credit_line": "<p>2022</p>\n<p>Tully Arnot, b. 1984, Gadigal land</p>\n<p>Lives and works, Hong Kong</p>\n<p>Free-roam virtual reality environment, spatial sound, scent, carpet, reading room</p>\n<p>Created in collaboration with Dr Josh Harle of Tactical Space Lab</p>", "headline_credit": "Australia, 2022", "video_links": [], "record_type": "work", "type": "Mixed reality", "is_on_display": true, "last_on_display_place": "ACMI: Gallery 3", "last_on_display_date": "2022-11-27", "is_context_indigenous": false, "material_description": "Oculus Quest 2 virtual reality headset, APK files, reading material, furniture and mixed media", "unpublished": false, "external": false, "first_production_date": "2022-01-01T11:00:00+11:00", "commissioned": true, "external_references": [], "brief_description": "<p>Epiphytes is a VR work by Australian artist Tully Arnot promoting a plant-based perspective.  Reflecting Arnot&#8217;s interest in how emergent technologies help form human understanding of the world, the visually diffuse nature of the work is deliberate. In Epiphytes, the primacy of sight is de-escalated, and sound and scent become key - used by Arnot to manipulate our bodily responses within the virtual space. &#8220;Epiphytes&#8221; encourages audiences to adjust the way we understand our own senses of perception by drawing our attention to alternative forms of plant communication and consciousness.</p>", "constellations_primary": [{"id": 356, "name": "Plants and people", "description": "Plants can be characters in stories, and we often project human qualities onto them, but how do they see the world?"}], "constellations_other": [], "recommendations": [], "title_for_label": "<p>Epiphytes</p>", "creator_credit_for_label": "<p>Tully Arnot</p>", "headline_credit_for_label": "<p>Australia, 2022</p>", "description": "<p>An epiphyte is an organism that lives on the surface of a plant, feeding on the air, water and natural refuse of its environment, in harmony with its surrounding ecosystem. Tully Arnot&#8217;s virtual reality work Epiphytes honours alternative forms of plant communication and consciousness, inviting us to question our own perception. The primacy of sight is minimised in favour of sound and scent, which Arnot uses to influence our bodily responses within the virtual space. Epiphytes reflects Arnot&#8217;s interest in how emergent technologies help form human understandings of the world.</p>\n<p>Situated within an abstracted representation of Arnot&#8217;s childhood backyard, the environment features a diffuse, shifting, magenta palette - suggestive of a phytomorphic (plant-based) interpretation of light and space. The work includes interviews with evolutionary ecologist Monica Gagliano, acoustic archaeologist Umashankar Manthravadi and echolocation teacher/blind researcher and activist Thomas Tajo.</p>\n<p>Tully Arnot is one of Australia&#8217;s leading visual artists. Arnot&#8217;s practice explores the effect that contemporary technology has on human relationships, looking at the isolating nature of our increasingly connected, but ultimately disconnected world. Themes of automation and simulation feature strongly in Arnot&#8217;s work, while relationships between human and vegetal worlds are explored as allegory for social or political structures.</p>\n<p>Arnot completed a Bachelor of Design, Honours (2009), and Master of Fine Arts (Research) (2014) at UNSWA+D. His international residencies include an exchange at Universitat der Kunste Berlin in 2013 with Gregor Schneider, as well as masterclasses at Donghua University Shanghai and Politechnico di Milano through Richard Goodwin&#8217;s Porosity Studio. In 2020-2021 Arnot was a City of Sydney Creative Fellowship artist, and his work has been shown widely both locally and internationally.</p>", "description_for_label": ["<p>An epiphyte is an organism that lives on the surface of a plant, feeding on the air, water and natural refuse of its environment, in harmony with its surrounding ecosystem. Tully Arnot&#8217;s virtual reality work Epiphytes honours alternative forms of plant communication and consciousness, inviting us to question our own perception. The primacy of sight is minimised in favour of sound and scent, which Arnot uses to influence our bodily responses within the virtual space. Epiphytes reflects Arnot&#8217;s interest in how emergent technologies help form human understandings of the world.</p>\n<p>Situated within an abstracted representation of Arnot&#8217;s childhood backyard, the environment features a diffuse, shifting, magenta palette - suggestive of a phytomorphic (plant-based) interpretation of light and space. The work includes interviews with evolutionary ecologist Monica Gagliano, acoustic archaeologist Umashankar Manthravadi and echolocation teacher/blind researcher and activist Thomas Tajo.</p>\n<p>Tully Arnot is one of Australia&#8217;s leading visual artists. Arnot&#8217;s practice explores the effect that contemporary technology has on human relationships, looking at the isolating nature of our increasingly connected, but ultimately disconnected world. Themes of automation and simulation feature strongly in Arnot&#8217;s work, while relationships between human and vegetal worlds are explored as allegory for social or political structures.</p>\n<p>Arnot completed a Bachelor of Design, Honours (2009), and Master of Fine Arts (Research) (2014) at UNSWA+D. His international residencies include an exchange at Universitat der Kunste Berlin in 2013 with Gregor Schneider, as well as masterclasses at Donghua University Shanghai and Politechnico di Milano through Richard Goodwin&#8217;s Porosity Studio. In 2020-2021 Arnot was a City of Sydney Creative Fellowship artist, and his work has been shown widely both locally and internationally.</p>"], "credit_line_for_label": "<p>2022</p>\n<p>Tully Arnot, b. 1984, Gadigal land</p>\n<p>Lives and works, Hong Kong</p>\n<p>Free-roam virtual reality environment, spatial sound, scent, carpet, reading room</p>\n<p>Created in collaboration with Dr Josh Harle of Tactical Space Lab</p>", "details": [{"label": "Duration", "display_values": ["00:12:00:00"]}, {"label": "Subject categories", "display_values": ["Digital Art", "Virtual Reality"]}, {"label": "Sound/audio", "display_values": ["Sound"]}, {"label": "Colour", "display_values": ["Colour"]}, {"label": "Object Types", "display_values": ["Virtual Reality Work"]}, {"label": "Materials", "display_values": ["Oculus Quest 2 virtual reality headset, APK files, reading material, furniture and mixed media"]}], "stats": {"tap_count": 345}, "links": [], "c