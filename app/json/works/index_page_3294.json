d_type": "group", "type": "Videogame", "is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2031-02-16", "is_context_indigenous": false, "material_description": "", "unpublished": false, "first_production_date": "2009-01-01T11:00:00+11:00", "brief_description": "<p>When Jenova Chen and Nicholas Clark designed <i>Flower</i>, they approached it as more of an interactive artwork than a traditional videogame.</p>", "constellations_primary": [{"id": 128, "name": "Flower on the wind", "description": "Explore the use of flowers in the moving image as an aesthetic and metaphor."}], "constellations_other": [], "recommendations": [], "title_for_label": "<p>Flower</p>", "creator_credit_for_label": "<p>thatgamecompany and Sony</p>", "headline_credit_for_label": "<p>United Kingdom, 2009</p>", "description": "<p>When Jenova Chen and Nicholas Clark designed <i>Flower</i>, they approached it as more of an interactive artwork than a traditional videogame.</p>\n<p><i>Flower</i> lets you control the wind as it blows a single flower petal around the stages. The petal interacts with various elements in the world, triggering wind turbines or rejuvenating the landscape.</p>\n<p>Rather than focus on traditional design principles of ‘fun’ or challenging gameplay, Chen and Clark instead worked to create a meditative experience they hoped would provoke positive emotions in the player.</p>", "description_for_label": ["<p>When Jenova Chen and Nicholas Clark designed <i>Flower</i>, they approached it as more of an interactive artwork than a traditional videogame.</p>\n<p><i>Flower</i> lets you control the wind as it blows a single flower petal around the stages. The petal interacts with various elements in the world, triggering wind turbines or rejuvenating the landscape.</p>\n<p>Rather than focus on traditional design principles of ‘fun’ or challenging gameplay, Chen and Clark instead worked to create a meditative experience they hoped would provoke positive emotions in the player.</p>"], "credit_line_for_label": "", "details": [{"label": "Curatorial section", "display_values": ["The Story of the Moving Image&nbsp;→ Games Lab&nbsp;→ 03. Cluster 3&nbsp;→ GL-03-C01"]}], "stats": {"tap_count": 7918}, "links": [], "creators_primary": [{"id": 1033638, "name": "thatgamecompany", "creator_id": 84333, "role": "creator", "role_id": 968, "is_primary": true}, {"id": 1033639, "name": "Sony", "creator_id": 71968, "role": "creator", "role_id": 968, "is_primary": true}], "creators_other": [], "video_links": [], "media_note": null, "holdings": [], "part_of": null, "parts": [], "part_siblings": [], "group": null, "group_works": [{"id": 117927, "acmi_id": "", "title": "Game screenshots", "title_annotation": "", "slug": "game-screenshots", "creator_credit": "", "credit_line": "", "headline_credit": "", "record_type": "group", "type": "Film", "is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2031-02-16", "is_context_indigenous": false, "material_description": "", "unpublished": false, "first_production_date": null}], "group_siblings": [], "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "183888", "production_places": [{"id": 10, "name": "United Kingdom", "slug": "united-kingdom"}], "production_dates": [{"date": "2009", "notes": "", "to_year": "2009"}], "labels": [57423]}]}