is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2031-02-16", "is_context_indigenous": false, "material_description": "", "unpublished": false, "external": false, "first_production_date": "2015-01-01T11:00:00+11:00", "commissioned": false, "external_references": [{"id": 46006, "source": {"name": "Wikidata", "slug": "wikidata"}, "source_identifier": "Q1757288"}]}], "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "180393", "eaas_environment_id": null, "production_places": [{"id": 3, "name": "Australia", "slug": "australia"}, {"id": 2, "name": "United States", "slug": "united-states"}], "production_dates": [{"date": "2015", "notes": "", "to_year": "2015"}], "labels": [46985]}, {"id": 118043, "acmi_id": "184289", "title": "Videogames and Us", "title_annotation": "", "slug": "videogames-and-us", "creator_credit": "ACMI", "credit_line": "<p>Additional research by Dr Dan Golding</p>", "headline_credit": "Australia, 2020", "video_links": [], "record_type": "work", "type": "Object", "is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2031-02-16", "is_context_indigenous": false, "material_description": "", "unpublished": false, "external": false, "first_production_date": "2020-01-01T11:00:00+11:00", "commissioned": false, "external_references": [], "brief_description": "<p>Since videogames were first invented in laboratories, our relationship to play has evolved. From arcades and the first home consoles to today’s blockbuster releases, mobile games and experimental indies, how we interpret and experience vieogames has changed alongside us.</p>", "constellations_primary": [{"id": 282, "name": "Arcades and AI", "description": "Using the godfather of computing, Alan Turing, as a starting point, this Constellation traces the evolution of AI representation on-screen and the way female androids, robots and holograms have been hyper sexualised."}], "constellations_other": [], "recommendations": [], "title_for_label": "<p>Videogames and Us</p>", "creator_credit_for_label": "<p>ACMI</p>", "headline_credit_for_label": "<p>Australia, 2020</p>", "description": "<p>Play is an essential part of who we are as individuals and as a collective. Since videogames were first invented by scientists in laboratories, our relationship to play has come a long way. From arcades and the first home consoles to today’s blockbuster releases, mobile games and experimental indies, videogames have evolved dramatically and so have we. How we interpret and experience videogames, and the communities and cultures spawned from them, reveals much about how society has shifted in the past five decades.</p>", "description_for_label": ["<p>Play is an essential part of who we are as individuals and as a collective. Since videogames were first invented by scientists in laboratories, our relationship to play has come a long way. From arcades and the first home consoles to today’s blockbuster releases, mobile games and experimental indies, videogames have evolved dramatically and so have we. How we interpret and experience videogames, and the communities and cultures spawned from them, reveals much about how society has shifted in the past five decades.</p>"], "credit_line_for_label": "<p>Additional research by Dr Dan Golding</p>", "details": [{"label": "Curatorial section", "display_values": ["The Story of the Moving Image&nbsp;→ Games Lab&nbsp;→ GL-05. Videogames Infographic&nbsp;→ GL-05-C01"]}, {"label": "Object Types", "display_values": ["2D Object"]}], "stats": {"tap_count": 125481}, "links": [], "creators_primary": [{"id": 2530583, "name": "ACMI", "creator_id": 6272, "creator_wikidata_id": null, "role": "creator", "role_id": 968, "is_primary": true}], "creators_other": [{"id": 2530584, "name": "Dan Golding", "creator_id": 84443, "creator_wikidata_id": null, "role": "researcher", "role_id": 950, "is_primary": false}], "media_note": null, "holdings": [], "part_of": null, "parts