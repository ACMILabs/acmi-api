[59540]}, {"id": 117871, "acmi_id": "B2004323", "title": "Spotlights: Ana Kokkinos", "title_annotation": "", "slug": "interview-with-ana-kokkinos", "creator_credit": "", "credit_line": "", "headline_credit": "Australia, 2020", "video_links": [], "record_type": "work", "type": "Film", "is_on_display": true, "last_on_display_place": "ACMI: Gallery 1", "last_on_display_date": "2031-02-16", "is_context_indigenous": false, "material_description": "", "unpublished": false, "external": false, "first_production_date": "2020-01-01T11:00:00+11:00", "commissioned": true, "external_references": [], "brief_description": "<p>An Australian film and TV director and screenwriter of Greek descent born in Melbourne. Kokkinos has directed three feature-length films and two short films including Head On (1998), The Book of Revelation (2006) and Blessed (2009).</p>", "constellations_primary": [{"id": 266, "name": "Ana Kokkinos", "description": "The films and TV works of Ana Kokkinos often explore the experiences of second-generation Australians, queer identities and class. Since beginning her career in the early 90s, those stories have started to appear more frequently on Australian screens."}], "constellations_other": [], "recommendations": [], "title_for_label": "<p>Ana Kokkinos</p>", "creator_credit_for_label": "", "headline_credit_for_label": "<p>Filmmaker</p>", "description": "<p>Ana Kokkinos is an Australian filmmaker and screenwriter of Greek descent born in Melbourne. Kokkinos has directed features and two shorts including <i>Head On</i> (1998), <i>The Book of Revelation</i> (2006) and <i>Blessed</i> (2009). Here she discusses diversity, particularly in relation to class and queerness.</p>", "description_for_label": ["<p>Ana Kokkinos is an Australian filmmaker and screenwriter of Greek descent born in Melbourne. Kokkinos has directed features and two shorts including <i>Head On</i> (1998), <i>The Book of Revelation</i> (2006) and <i>Blessed</i> (2009). Here she discusses diversit