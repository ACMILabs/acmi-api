orks": [], "group_siblings": [], "source": {"name": "Vernon", "slug": "vernon"}, "source_identifier": "24278", "eaas_environment_id": null, "production_places": [{"id": 10, "name": "United Kingdom", "slug": "united-kingdom"}], "production_dates": [{"date": "1949", "notes": "Produced", "to_year": "1949"}], "labels": [8136], "external_references": []}, {"id": 83920, "acmi_id": "305196", "title": "How to play cricket by Don Bradman", "title_annotation": "", "slug": "how-to-play-cricket-by-don-bradman", "creator_credit": "Bradman Museum", "credit_line": "", "headline_credit": "Australia, 1993", "record_type": "work", "type": "Film", "is_on_display": false, "last_on_display_place": null, "last_on_display_date": null, "is_context_indigenous": false, "material_description": "", "unpublished": false, "external": false, "first_production_date": "1993-01-01T11:00:00+11:00", "brief_description": "<p>This unique film was first produced in England in 1934 with the script written and spoken by Don Bradman. It has been updated with comments by Sir Donald, filmed at his home in Adelaide in May 1993. Provides a rare opportunity for cricket lovers to study the master batsman&#8217;s technique in detail, and is an invaluable coaching aid to any aspiring cricketer. Bradman&#8217;s test match career was an outstanding example of sporting brilliance; his career total of runs was 6,996 with an average of 99.9, including 29 centuries. Aggression, initiative and improvisation were the hallmarks of his playing and this footage, filmed on the London Cricket Ground in 1934, captures his style perfectly, shows the complete shot and stroke broken down into successive movements and as such provides an invaluable visual description of how to achieve maximum effectiveness and efficiency of movements when batting in a game.</p>", "constellations_primary": [], "constellations_other": [], "recommendations": [], "title_for_label": "<p>How to play cricket by Don Bradman</p>", "creator_credit_for_label": "<p>Bradman Museum</p>", "headline_credit_for_label": "<p>Australia, 1993</p>", "description": "<p>This unique film was first produced in England in 1934 with the script written and spoken by Don Bradman. It has been updated with comments by Sir Donald, filmed at his home in Adelaide in May 1993. Provides a rare opportunity for cricket lovers to study the master batsman&#8217;s technique in detail, and is an invaluable coaching aid to any aspiring cricketer. Bradman&#8217;s test match career was an outstanding example of sporting brilliance; his career total of runs was 6,996 with an average of 99.9, including 29 centuries. Aggression, initiative and improvisation were the hallmarks of his playing and this footage, filmed on the London Cricket Ground in 1934, captures his style perfectly, shows the complete shot and stroke broken down into successive movements and as such provides an invaluable visual description of how to achieve maximum effectiveness and efficiency of movements when batting in a game.</p>", "description_for_label": ["<p>This unique film was first produced in England in 1934 with the script written and spoken by Don Bradman. It has been updated with comments by Sir Donald, filmed at his home in Adelaide in May 1993. Provides a rare opportunity for cricket lovers to study the master batsman&#8217;s technique in detail, and is an invaluable coaching aid to any aspiring cricketer. Bradman&#8217;s test match career was an outstanding example of sporting brilliance; his career total of runs was 6,996 with an average of 99.9, including 29 centuries. Aggression, initiative and improvisation were the hallmarks of his playing and this footage, filmed on the London Cricket Ground in 1934, captures his style perfectly, shows the complete shot and stroke broken down into successive movements and as such provides an invaluable visual description of how to achieve maximum effectiveness and efficiency of movements when batting in a game.</p>"], "credit_line_for_